*
*	Program Fast Gadget.
*
_l	SET	TBFG_LEFTEDGE+0		; Pick Area. ( NOT ASSIGNED )
_t	SET	TBFG_TOPEDGE+0
_w	SET	0
_h	SET	0
*
_ml	SET	TBFG_LEFTEDGE+512	; Drawing Area.
_mt	SET	TBFG_TOPEDGE+346+31+14
_mw	SET	128
_mh	SET	11
*
	XDEF	LUT_PROGRAMFG
LUT_PROGRAMFG:
	DC.L	LUT_OkFG		; gg_NextGadget
	DC.W	_l			; gg_LeftEdge
	DC.W	_t			; gg_TopEdge
	DC.W	_w			; gg_Width
	DC.W	_h			; gg_Height
	DC.W	GADGHNONE		; gg_Flags
	DC.W	GADGIMMEDIATE		; gg_Activation
	DC.W	BOOLGADGET		; gg_GadgetType
	DC.L	0,0,0			; gg_GadgetRender, Select, Text.
	DC.L	0,0			; Mutual Exclude, Special Info.
	DC.W	0			; Gadget ID.
	DC.L	0			; General Purpose Data.
	DC.W	DISPLAYMODE4		; FG_ModeType
	DC.W	_mw/16-1		; FG_WWidth
	DC.W	_mh-1			; FG_Height
	DC.W	DISPLAYWIDTH/8-_mw/8	; FG_Modulus
	DC.L	DISPLAYWIDTH/8*_mt+_ml/8 ;FG_Offset
LUT_PROGRAM_Img_PTRS
	DC.L	LUT_PROGRAM_BT		; FG_Data
	DC.W	1			; FG_EntrySize
	DC.W	0			; FG_PTRIndex
	DC.L	LUT_PROGRAM_Img_PTRS	; FG_PTRTable
	DC.L	LUT_DoHighLiteOnOff	; FG_Function
	DC.B	0			; FG_LoadFlag
	DC.B	0			; FG_DispFlag
	DC.B	0			; FG_MouseFlag
	DC.B	0			; FG_HiLiteVal
	DC.W	0			; FG_IndexID
	DC.W	0			; FG_HiLiteMask
	DC.W	0			; FG_BorderCon
	DC.W	0			; FG_TopSize
	DC.W	0			; FG_BotSize
	DC.W	0			; FG_LeftWSize
	DC.W	0			; FG_RightWSize
	DC.W	0			; FG_LeftMask
	DC.W	0			; FG_RightMask
	DC.W	0			; FG_Extra1
*
*	Switcher Fast Gadget.
*
_l	SET	TBFG_LEFTEDGE+528+32
_t	SET	TBFG_TOPEDGE-5
_w	SET	80
_h	SET	20
*
	XDEF	LUT_OkFG
LUT_OkFG:
	DC.L	LUT_TAKEFG		; gg_NextGadget
	DC.W	_l			; gg_LeftEdge
	DC.W	_t			; gg_TopEdge
	DC.W	_w			; gg_Width
	DC.W	_h			; gg_Height
	DC.W	GADGHNONE		; gg_Flags
	DC.W	GADGIMMEDIATE		; gg_Activation
	DC.W	BOOLGADGET		; gg_GadgetType
	DCB.B	gg_SIZEOF-gg_GadgetRender,0 ; clear rest of Gadget structure
	DC.W	DISPLAYMODE4		; FG_ModeType
	DC.W	_w/16-1			; FG_WWidth
	DC.W	_h-1			; FG_Height
	DC.W	DISPLAYWIDTH/8-_w/8	; FG_Modulus
	DC.L	DISPLAYWIDTH/8*_t+_l/8	; FG_Offset
LUT_Ok_Img_PTRS
	DC.L	LUT_OK_BT		; FG_Data
	DC.W	1			; FG_EntrySize
	DC.W	0			; FG_PTRIndex
	DC.L	LUT_Ok_Img_PTRS		; FG_PTRTable
	DC.L	LUT_DoHighLiteOnOff	; FG_Function
	DC.B	0			; FG_LoadFlag
	DC.B	0			; FG_DispFlag
	DC.B	0			; FG_MouseFlag
	DC.B	$FF			; FG_HiLiteVal
	DC.W	0			; FG_IndexID
	DC.W	0			; FG_HiLiteMask
	DC.W	0			; FG_BorderCon
	DC.W	0			; FG_TopSize
	DC.W	0			; FG_BotSize
	DC.W	0			; FG_LeftWSize
	DC.W	0			; FG_RightWSize
	DC.W	0			; FG_LeftMask
	DC.W	0			; FG_RightMask
	DC.W	0			; FG_Extra1
