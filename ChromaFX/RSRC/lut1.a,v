	XDEF	LUT_FirstFG
LUT_FirstFG:
*
*	ProgramKey 1 Fast Gadget.
*
_l	SET	TBFG_LEFTEDGE+512
_t	SET	TBFG_TOPEDGE+320+31+14
_w	SET	32
_h	SET	24
*
	XDEF	LUT_ProgramKey1FG
LUT_ProgramKey1FG:
	DC.L	LUT_ProgramKey2FG	; gg_NextGadget
	DC.W	_l			; gg_LeftEdge
	DC.W	_t			; gg_TopEdge
	DC.W	_w			; gg_Width
	DC.W	_h			; gg_Height
	DC.W	GADGHNONE		; gg_Flags
	DC.W	GADGIMMEDIATE		; gg_Activation
	DC.W	BOOLGADGET		; gg_GadgetType
	DCB.B	gg_SIZEOF-gg_GadgetRender,0 ; clear rest of Gadget structure
	DC.W	DISPLAYMODE4		; FG_ModeType
	DC.W	_w/16-1			; FG_WWidth
	DC.W	_h-1			; FG_Height
	DC.W	DISPLAYWIDTH/8-_w/8	; FG_Modulus
	DC.L	DISPLAYWIDTH/8*_t+_l/8	; FG_Offset
LUT_ProgramKey1_Img_PTRS
	DC.L	LUT_PROGRAMKEY1_BT	; FG_Data
	DC.W	1			; FG_EntrySize
	DC.W	0			; FG_PTRIndex
	DC.L	LUT_ProgramKey1_Img_PTRS ;FG_PTRTable
	DC.L	LUT_ProgramPannel	; FG_Function
	DC.B	0			; FG_LoadFlag
	DC.B	0			; FG_DispFlag
	DC.B	0			; FG_MouseFlag
	DC.B	$FF			; FG_HiLiteVal
	DC.W	0			; FG_IndexID
	DC.W	0			; FG_HiLiteMask
	DC.W	0			; FG_BorderCon
	DC.W	0			; FG_TopSize
	DC.W	0			; FG_BotSize
	DC.W	0			; FG_LeftWSize
	DC.W	0			; FG_RightWSize
	DC.W	0			; FG_LeftMask
	DC.W	0			; FG_RightMask
	DC.W	0			; FG_Extra1
*
*	ProgramKey 2 Fast Gadget.
*
_l	SET	TBFG_LEFTEDGE+544
_t	SET	TBFG_TOPEDGE+320+31+14
_w	SET	32
_h	SET	24
*
	XDEF	LUT_ProgramKey2FG
LUT_ProgramKey2FG:
