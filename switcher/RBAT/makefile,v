.ONERROR:
   @@echo >ram:temp2  "echo ERROR ERROR ERROR ERROR ERROR ERROR ERROR"
   @@echo >>ram:temp2 "echo vvvvvvvvvvvvvvvvvvvvvvvvvvvvvvvvvvvvvvvvv"
   @@list >>ram:temp2 ram:make.errors\#? lformat "type ram:%s"
   @@echo >>ram:temp2 "echo ^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^"
   @@echo >>ram:temp2 "echo ERROR ERROR ERROR ERROR ERROR ERROR ERROR"
   @@rx "say d2c(7)"
   @@execute ram:temp2
   @@delete ram:temp2 quiet

# end of makefile
