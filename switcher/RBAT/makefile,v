2.128
log
@added art.a
@
text
@#********************************************************************
#* $RCSfile: makefile,v $
#*
#* Copyright (c)1992 NewTek, Inc.
#* Confidental and Proprietary. All rights reserved.
#*
#* $Id: makefile,v 2.127 1995/08/31 17:36:38 Flick Exp Holt $
#*
#*********************************************************************

REV = 4.06

# -L = linkable code (else its executable), -D = debug
#HIFLAGS	= -Isw:sinc/ -L -O
#HIDBNOL	= -Isw:sinc/ -D -O
#HIDEBUG = -Isw:sinc/ -L -D -O

DOL =	$$

HIDEBUG = -Isw:sinc/ -L -D LINE -O

DEST = VT:exe/Toaster/

#***********************************************************
# Compiling rules. optimized object, runs only with switcher

SAS_OPT = >ram:Make.errors RESETOPTIONS DEBUG=L OPTIMIZE ERRORREXX \
	 UNSCHAR STRMERGE IDIR=include.h: IDIR=scinc: IDIR=sw:inc/

SAS_ASM = DATA=faronly NOSTACKCHECK
.c.o:
	sc $(SAS_OPT) OBJNAME=ram:temp $(SAS_ASM) DEFINE=RELEASE DEFINE=FINAL_CODE $*.c
# 	@@NextTime $*.c ram:temp
	@@copy ram:temp $@@ quiet clone
	@@delete ram:temp quiet

#***********************************************************
# Include striping rules
.i.i:
	strip $*.i ram:temp
#	@@NextTime $*.i ram:temp
	@@copy ram:temp $@@ quiet clone
	@@delete ram:temp quiet

#***********************************************************
# Assembly rules for non-AA (default) modules
.a.o:
	echo "Creating $@@"
	GenAm >ram:Make.errors $*.a $(HIDEBUG)ram:temp -e ECSCHIPS=1
#	@@NextTime $*.a ram:temp
	@@copy ram:temp $@@ quiet clone
	@@delete ram:temp quiet

#***********************************************************
# Assembly rules for AA modules
.a.AA_o:
	echo "Creating $@@"
	GenAm >ram:Make.errors $*.a $(HIDEBUG)ram:temp -e AACHIPS=1
#	@@NextTime $*.a ram:temp
	@@copy ram:temp $@@ quiet clone
	@@delete ram:temp quiet

#***********************************************************
# Assembly rules for non-AA old Toasters (3.0)
.a.OLD_o:
	echo "Creating $@@"
	GenAm >ram:Make.errors $*.a $(HIDEBUG)ram:temp -e OLDTOASTER=1
#	@@NextTime $*.a ram:temp
	@@copy ram:temp $@@ quiet clone
	@@delete ram:temp quiet

#***********************************************************

SWINCS = sw:sinc/vtdebug.i sw:sinc/serialdebug.i sw:sinc/instinct.i sw:sinc/eflib.i

Switcher: $(SWINCS)\
	ECS AA 3.0 $(DEST)VideoToaster $(DEST)VideoToaster.AA\
	sw:doc/CommonRGB.doc sw:doc/ARexx.doc $(DEST)Switcher_Support/HS.Default $(DEST)Switcher_Support/HS\
	sw:obj/ToasterLib.o sw:obj/ToastGlue.o

# $(DEST)auxlibs/mtslicerequester.library
# $(DEST)VTD $(DEST)VTI $(DEST)VTA removed from above

ram:NeverExists: ECS AA 3.0 $(DEST)VideoToaster $(DEST)VideoToaster.AA\
	$(DEST)Switcher_Support/HS.Default $(DEST)Switcher_Support/HS
  quit 20

sw:bat/LastUpdateDate:
	date >sw:bat/LastUpdateDate

#*******************************************************************

# $(DEST)VTD $(DEST)VTI $(DEST)VTA removed from above

AA: $(DEST)Switcher.main.AA

ECS: $(DEST)Switcher.main

3.0: $(DEST)Switcher.main.3.0

TestStation: $(SWINCS)\
	$(DEST)Switcher.main.Master $(DEST)Switcher.main.Slave

SWOBJS =\
	sw:obj/LowMem.o sw:obj/MBDispatch.o \
	sw:obj/SoftSprite.o sw:obj/Lib.o sw:obj/Misc.o \
	sw:obj/ables.o sw:obj/LumKeyFGs.o sw:obj/MainFGs.o\
	sw:obj/NumPadFGs.o sw:obj/FrameFGs.o sw:obj/TransFGs.o\
	sw:obj/FMCountFGs.o sw:obj/ClipFGs.o sw:obj/TBarFGs.o sw:obj/Main.o\
	sw:obj/Preview.o sw:obj/LumKey.o sw:obj/GridSel.o sw:obj/Auto.o sw:obj/Take.o\
	sw:obj/TBar.o sw:obj/NumPad.o sw:obj/FMCount.o sw:obj/Clip.o\
	sw:obj/eflib.o sw:obj/ntm.o sw:obj/sine.o sw:obj/CGSliceCode.o\
	sw:obj/pixelize.o sw:obj/grabber.o sw:obj/grab.o sw:obj/frame.o sw:obj/DLData.o\
	sw:obj/FrameRead.o sw:obj/FrameWrite.o sw:obj/InitEFX.o\
	sw:obj/Freeze.o sw:obj/Horizontal.o sw:obj/TBEFXCode.o\
	sw:obj/MTSliceCode.o sw:obj/MTSliceData.o\
	sw:obj/ClipKeyMode.o sw:obj/Buttons.o\
	sw:obj/Requesters.o sw:obj/KeyLogic.o sw:obj/bgcolor.o\
	sw:obj/PaintSlice.o sw:obj/DeMotion.o sw:obj/Encrypt.o\
	sw:obj/NewText.o sw:obj/FastBlitter.o sw:obj/ARexxHandler.o\
	sw:obj/LightWaveSlice.o\
	sw:obj/SwitcherGPI.o sw:obj/HardSets.o sw:obj/HardSetsData.o sw:obj/LineZero.o\
	sw:obj/RGB2YIQ.o sw:obj/YIQ2COMP.o sw:obj/SmartSend.o\
	sw:obj/OutputGPI.o sw:obj/LUTSlice.o sw:obj/KbdState.o\
	sw:obj/CommonRGB.o sw:obj/FastSeg.o sw:obj/DosBuffer.o\
	sw:obj/AnimLibrary.o sw:obj/IFFuncompress.o sw:obj/AnimRam.o sw:obj/HAMPreviewhandler.o\
	sw:obj/Memory.o sw:obj/FileCopy.o sw:obj/sequence.o sw:obj/PlayAnim.o \
	sw:obj/AnimUnpack.o sw:obj/HamAnim.o sw:obj/AnimProcess.o sw:obj/AnimWrite.o \
	sw:obj/AnimCompress.o sw:obj/Palette.o \
	sw:obj/Anim7BV.o sw:obj/Anim7LH.o sw:obj/Anim7LV.o sw:obj/Anim7WH.o \
	sw:obj/Anim7WV.o sw:obj/Anim7BH.o sw:obj/Anim8.o sw:obj/ASLfileReq.o\
	sw:obj/AnimatedEffects.o \
	sw:obj/popup.o sw:obj/oras.o sw:obj/ckpt.o sw:obj/stdmem.o sw:obj/panic.o sw:obj/afail.o\
	sw:obj/AudioFX.o sw:obj/AudioPlay.o sw:obj/CStubs.o sw:obj/AnimFXHandler.o\
	sw:obj/denise.o sw:obj/PEComm.o sw:obj/AlgoFX.o sw:obj/CroutonLoad.o sw:obj/Flyer.o\
	sw:obj/FSIcon.o sw:obj/CroutonHandler.o sw:obj/CRFXHandler.o \
	sw:obj/Crawl.o sw:obj/ScrollRender.o sw:obj/Scroll.o sw:obj/NewCrawl.o \
	sw:obj/SerialDebug.o sw:obj/art.o

$(DEST)Switcher.main: sw:obj/Switcher.o sw:bat/LastUpdateDate $(SWINCS) $(SWOBJS)\
	sw:obj/XProgram.o sw:obj/CopChip.o sw:obj/cop.o sw:obj/vthand.o\
	sw:obj/AutoCal.o sw:obj/AnimWipe.o sw:obj/AnimFileIO.o\
	sw:obj/dynam.o sw:obj/bars.o sw:obj/FastGadgets.o sw:obj/Greet.o\
	sw:obj/Adjust.o sw:obj/AdjustData.o sw:obj/KLUDGE.o sw:obj/SliceFGs.o\
	sw:obj/GridSelFGs.o sw:obj/PreviewFGs.o sw:obj/FrameSlice.o sw:obj/CGSupport.o

	GenAm >ram:Make.errors sw:src/Version.a $(HIDEBUG)ram:temp
	@@copy ram:temp sw:obj/Version.o quiet clone
	@@Echo >ram:vtemp "$(DOL)VER: Video Toaster (VT4000/A2000) $(REV) " noline
	@@date >>ram:vtemp
	@@getenv >>ram:vtemp USERNAME
	@@insertfile 40 ram:vtemp sw:obj/Version.o
	@@echo >ram:Make.errors "SLINK Failed!"
	slink WITH sw:bat/WithFile
	@@echo >ram:Make.errors
	@@copy ram:temp $@@ quiet clone
	@@delete ram:temp quiet

$(DEST)Switcher.main.3.0: sw:obj/Switcher.OLD_o sw:bat/LastUpdateDate $(SWINCS) $(SWOBJS)\
	sw:obj/XProgram.o sw:obj/CopChip.o sw:obj/cop.o sw:obj/vthand.o\
	sw:obj/AutoCal.o sw:obj/AnimWipe.o sw:obj/AnimFileIO.o\
	sw:obj/dynam.o sw:obj/bars.o sw:obj/FastGadgets.o sw:obj/Greet.OLD_o\
	sw:obj/KLUDGE.OLD_o sw:obj/SliceFGs.o\
	sw:obj/GridSelFGs.o sw:obj/PreviewFGs.OLD_o sw:obj/FrameSlice.o sw:obj/CGSupport.o

	GenAm >ram:Make.errors sw:src/Version.a $(HIDEBUG)ram:temp
	@@copy ram:temp sw:obj/Version.o quiet clone
	@@Echo >ram:vtemp "$(DOL)VER: Video Toaster (VT2000/A2000) $(REV) " noline
	@@date >>ram:vtemp
	@@getenv >>ram:vtemp USERNAME
	@@insertfile 40 ram:vtemp sw:obj/Version.o
	@@echo >ram:Make.errors "SLINK Failed!"
	slink WITH sw:bat/WithFile.3.0
	@@echo >ram:Make.errors
	@@copy ram:temp $@@ quiet clone
	@@delete ram:temp quiet

