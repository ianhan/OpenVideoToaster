 IFEQ 1
****************************************************************
* HandleAlphaAuto(->TB)
* d0=auto/unauto, a0->logicTbl, a2->data, a3->FG, a4->EFXlib, a5->TB
HandleAlphaAuto	movem.l	d0-d6,-(sp)

;	DUMPREG	<HandleAlphaAuto>	
;;	DUMPMSG	<HandleAlphaAuto>
;;	move.l	#$10000,d3
	move.l	#$20000,d3		;;;TEST080495

;; in test as of 060595 count was 3C 	
	GET.w	TB_FCountSec,d2	;FrameCount for effect (was Pri pre 1/13/94)
;	DUMPREG	<d2 framecount>						
	divu	d2,d3		;incremental time

	swap	d3		;clear upper word
	clr.w	d3
	swap	d3

	tst.w	d0
	beq.s	17$
	GET.W	TB_TValSec,d0	;unauto
;	DUMPREG	<TB_TValSec,d0>
	mulu	d2,d0
	divu	#TValMax,d0	;round down to #frames left

	move.w	d3,d4
	mulu	d0,d4
	move.w	d0,d2		;frame counter
	neg.l	d3
	add.l	d3,d4		;do next frame from current time
	bra	18$

17$	move.w	#TValMax,d0
;	DUMPREG	<TValMax--D0>

	sub.w	TB_TValSec(a5),d0
;	DUMPREG	<D0 #TValMax-TB_TValSec(a5) >

	MOVE.L	#TValMax/3,D0	;; TESTING42095

;;	MOVE.L	#$1fc,D0
 		
	mulu	d2,d0
	divu	#TValMax,d0	;rounded down to #frames left
;	DUMPREG	<divu    #TValMax,d0>

	move.w	d3,d4
	sub.w	d0,d2		;# of frames from start
	mulu	d2,d4
	add.l	d3,d4		;do next frame from current time
	move.w	d0,d2
18$	
;;	CALLTL	SoftSpriteOff	;incase theres any FG rendering within Auto handler
	CALLTL	InterruptsOff

;	DUMPREG	<Begining key fade loop>
	IFD	AACHICKS	;;THIS IS THE CORRECT FADE BEFORE0580495!!!
*	move.l	#$01eb0013,d0
*	move.l	#$001f0013,d0
	move.l	#$00ff0013,d0
*	move.l	#$1,d1
	move.l	#$1,d1		;?
	move.l	#$1f,d2		;number of steps?($13)	These values for Fadeoff
	move.l	#$0844,d3	;xx00 xx=inc($444)
	move.l	#$002b,d4	;xx00 xx=start val($b32b)

	tst.w	FadeOffFlag	;Normally it will be fade on, these are for on
	bne.s	45$
	move.l	#$20,d2		;number of steps?($13)
	move.l	#$0844,d3	;xx00 xx=inc($1f4f)
	move.l	#$002b,d4	;xx00 xx=start val($502b)
	ENDC

	IFD	AACHIPS
*	move.l	#$01eb0013,d0
*	move.l	#$001f0013,d0
	move.l	#$00ff0013,d0
*	move.l	#$1,d1
	move.l	#$1,d1		;?
	move.l	#$1f,d2		;number of steps?($13)	These values for Fadeoff
	move.l	#$0844,d3	;xx00 xx=inc($444)
	move.l	#$002b,d4	;xx00 xx=start val($b32b)

	tst.w	FadeOffFlag	;Normally it will be fade on, these are for on
	bne.s	45$
	move.l	#$20,d2		;number of steps?($13)
	move.l	#$0844,d3	;xx00 xx=inc($1f4f)
	move.l	#$002b,d4	;xx00 xx=start val($502b)
	ENDC




	IFND	AACHIPS
*	move.l	#$01eb0013,d0
*	move.l	#$001f0013,d0
	move.l	#$00ff0013,d0
*	move.l	#$1,d1
	move.l	#$1,d1		;?
	move.l	#$1f/2,d2		;number of steps?($13)	These values for Fadeoff
	move.l	#$0F44,d3	;xx00 xx=inc($444)
	move.l	#$002b,d4	;xx00 xx=start val($b32b)

	tst.w	FadeOffFlag	;Normally it will be fade on, these are for on
	bne.s	45$
	move.l	#$10,d2		;number of steps?($13)
	move.l	#$1044,d3	;xx00 xx=inc($1f4f)
	move.l	#$002b,d4	;xx00 xx=start val($502b)
	ENDC

45$
	bra	50$
20$	move.l	d4,d5		;THIS IS THE KEYFADE LOOP
*	DUMPMSG	<KEY FADING.....>
;	DUMPREG	<in key fade loop>		
	asr.l	#7,d5
	bge.s	25$
	moveq	#0,d4
	moveq	#0,d5
25$	move.l	d4,d0

	cmpi.w	#TValMax,d5
	bls.s	35$
	move.w	#TValMax,d5
35$	PUT.w	d5,TB_TValSec

;handle keys because the hardware depends on them!
	move.l	d0,d1
;	DUMPREG	<BEFORE ProcessDuring>
;;	CALLEF  ProcessDuring		***************<<<<<<<<<<<<<<
;	DUMPREG	<AFTER 	ProcessDuring>
	exg	d1,d0		;use ProcessDurings elh flag
;	DUMPREG	<CALLING HandleAlpha>	
	bsr	HandleAlpha		***************<<<<<<<<<<<<<<

	btst.b	#WAITABORT_BIT,TB_Flags3(a5)
	bne.s	39$

	add.l	d3,d4
50$	dbra	d2,20$
*	move.l	#$ffffffff,d0
*	bsr	HandleAlpha
********* TEST *********	
	move.l	#$00010000,d0
	move.l	#$00010000,d1
	bsr	HandleAlpha
*********


;	DUMPREG	<after dbra in key fade loop>		
39$		
;	DUMPREG	<after 39$ of key fade loop>	
	CALLTL	InterruptsOn
;;	CALLTL	SoftSpriteOn

*	DEBUGMSG	DBKH,<HandleAlphaAuto Done\ >

***!!! DANGER Will Robinson this causes an enforcer hit somewhere doon
;;	CALLTL	KillAlphaKey
;;	move.l	0,TB_CurrentAlphaBM(a5)

a1061 2
 ENDC

a1063 1
 IFEQ 0
a1098 1

d1101 1
d1104 5
a1108 3
	CALLTL	InstallAVEIdoELH		;<<<THE CHANGE TAKE PLACE HERE<<<

;	CALLTL	UpdateTBar
a1112 7
 ENDC






a1115 1
 IFEQ 1
a1116 109
* HandleAlphaAuto(->TB)
* d0=auto/unauto, a0->logicTbl, a2->data, a3->FG, a4->EFXlib, a5->TB
HandleAlphaAuto	movem.l	d0-d6,-(sp)
;	DUMPREG	<HandleAlphaAuto>	
;;	DUMPMSG	<HandleAlphaAuto>
		move.l	#$10000,d3
		GET.w	TB_FCountSec,d2	;FrameCount for effect (was Pri pre 1/13/94)
;	DUMPREG	<d2 framecount>						

		divu	d2,d3		;incremental time

		swap	d3		;clear upper word
		clr.w	d3
		swap	d3

		tst.w	d0
		beq.s	17$
		GET.W	TB_TVal(Sec,d0	;unauto
		mulu	d2,d0
		divu	#TValMax,d0	;round down to #frames left

		move.w	d3,d4
		mulu	d0,d4
		move.w	d0,d2		;frame counter
		neg.l	d3
		add.l	d3,d4		;do next frame from current time
		bra	18$

17$		move.w	#TValMax,d0
;	DUMPREG	<TValMax--D0>

		sub.w	TB_TValSec(a5),d0
;	DUMPREG	<D0 #TValMax-TB_TValSec(a5) >

;;		MOVE.L	#TValMax/3,D0	;; TESTING42095

		MOVE.L	#255,D0


 		
		mulu	d2,d0
		divu	#TValMax,d0	;rounded down to #frames left

		move.w	d3,d4
		sub.w	d0,d2		;# of frames from start
		mulu	d2,d4
		add.l	d3,d4		;do next frame from current time
		move.w	d0,d2


18$	
;;		CALLTL	SoftSpriteOff	;incase theres any FG rendering within Auto handler
		CALLTL	InterruptsOff

;	DUMPREG	<Begining key fade loop>


		bra	50$
20$		move.l	d4,d5		;THIS IS THE KEYFADE LOOP
*	DUMPMSG	<KEY FADING.....>
;	DUMPREG	<in key fade loop>		
		asr.l	#7,d5
		bge.s	25$
		moveq	#0,d4
		moveq	#0,d5
25$		move.l	d4,d0

		cmpi.w	#TValMax,d5
		bls.s	35$
		move.w	#TValMax,d5
35$		PUT.w	d5,TB_TValSec

;handle keys because the hardware depends on them!
		move.l	d0,d1
;	DUMPREG	<BEFORE ProcessDuring>
		CALLEF  ProcessDuring	*****<<<<<
;	DUMPREG	<AFTER 	ProcessDuring>
		exg	d1,d0		;use ProcessDurings elh flag
		bsr	HandleAlpha

		btst.b	#WAITABORT_BIT,TB_Flags3(a5)
		bne.s	39$

		add.l	d3,d4
50$		dbra	d2,20$
;	DUMPREG	<after dbra in key fade loop>		
39$		
;	DUMPREG	<after 39$ of key fade loop>	
		CALLTL	InterruptsOn
	;;	CALLTL	SoftSpriteOn


*	DEBUGMSG	DBKH,<HandleAlphaAuto Done\ >

***!!! DANGER Will Robinson this causes an enforcer hit somewhere doon
;;	CALLTL	KillAlphaKey
;;	move.l	0,TB_CurrentAlphaBM(a5)

		movem.l	(sp)+,d0-d6
		rts
 ENDC







****************************************************************
d1124 16
a1139 1
	movem.l	d0/d2,-(sp)
d1141 21
a1161 12
	swap	d0
	clr.w	d0
	divu.w	#TValMax,d0
	bvc.s	12$
	move.l	#$10000,d0
	bra.s	13$
12$	swap	d0
	clr.w	d0	;through away fraction
	swap	d0
13$	bsr.s	HandleAlpha
	movem.l	(sp)+,d0/d2
	rts
d1164 2
d1178 1
a1187 1
*	LSR.L	#6,D0
a1192 2
*	SUB.W	#8,D0
*	DUMPREG	<handlealpha>
a1209 5
***	GET.l	TB_ColorCycle,d0
***	beq.s	70$
***	bset.b	#0,TB_CycleFlags(a5)
;;	CALLTL	VBServer1		;do the colorcycling VertIntServer

d1238 1
a1238 1
	movem.l	(sp)+,d0-d1/a0-a1
d1243 3
a1245 2

********************************************************
@


