          CPMOVEDC   $008,color04   I=0 B=1 G=0 R=0
          CPMOVEDC   $808,color05   I=0 B=1 G=0 R=1
          CPMOVEDC   $088,color06   I=0 B=1 G=1 R=0
          CPMOVEDC   $888,color07   I=0 B=1 G=1 R=1
          CPMOVEDC   $003,color08   I=1 B=0 G=0 R=0
          CPMOVEDC   $f03,color09   I=1 B=0 G=0 R=1
          CPMOVEDC   $0f3,color10   I=1 B=0 G=1 R=0
          CPMOVEDC   $ff3,color11   I=1 B=0 G=1 R=1
          CPMOVEDC   $00f,color12   I=1 B=1 G=0 R=0
          CPMOVEDC   $f0f,color13   I=1 B=1 G=0 R=1
          CPMOVEDC   $0ff,color14   I=1 B=1 G=1 R=0
          CPMOVEDC   $fff,color15   I=1 B=1 G=1 R=1
	ENDC

	CPMOVEDC	((424>>1)&$fc),ddfstop
	CPMOVEDC	((7<<4)!7),bplcon1		;scroll

SWOS4:	EQU	*+2-SafeWriteCopList1
	CPMOVEDC	0,bpl1mod	;-96 for sync write, -4 for 736 wide, 0 for 768 wide, fill in later #
	CPMOVEDC	0,bpl2mod	; #
	
SWOS5:	EQU	*+2-SafeWriteCopList1
 	CPMOVEDC	$0000,bpl1pth       ;fill in later #
	CPMOVEDC	$0000,bpl1ptl       ;fill in later #
	CPMOVEDC	$0000,bpl2pth       ;fill in later #
	CPMOVEDC	$0000,bpl2ptl       ;fill in later #
