          CPMOVEDC   $000,color21       ;fill in later #
          CPMOVEDC   $000,color22       ;fill in later #
          CPMOVEDC   $000,color23       ;fill in later #
          CPMOVEDC   $000,color24       ;fill in later #
          CPMOVEDC   $000,color25       ;fill in later #
          CPMOVEDC   $000,color26       ;fill in later #
          CPMOVEDC   $000,color27       ;fill in later #
          CPMOVEDC   $000,color28       ;fill in later #
          CPMOVEDC   $000,color29       ;fill in later #
          CPMOVEDC   $000,color30       ;fill in later #
          CPMOVEDC   $000,color31       ;fill in later #

	  AAbplcon3  0,0
	  CPMOVEDC   AAvalue,bplcon3
          CPMOVEDC   $000,color01       ;fill in later #
          CPMOVEDC   $000,color02       ;fill in later #
          CPMOVEDC   $000,color03       ;fill in later #
          CPMOVEDC   $000,color04       ;fill in later #
          CPMOVEDC   $000,color05       ;fill in later #
          CPMOVEDC   $000,color06       ;fill in later #
          CPMOVEDC   $000,color07       ;fill in later #
          CPMOVEDC   $000,color08       ;fill in later #
          CPMOVEDC   $000,color09       ;fill in later #
          CPMOVEDC   $000,color10       ;fill in later #
          CPMOVEDC   $000,color11       ;fill in later #
          CPMOVEDC   $000,color12       ;fill in later #
          CPMOVEDC   $000,color13       ;fill in later #
          CPMOVEDC   $000,color14       ;fill in later #
          CPMOVEDC   $000,color15       ;fill in later #
          CPMOVEDC   $000,color16       ;fill in later #
          CPMOVEDC   $000,color17       ;fill in later #
          CPMOVEDC   $000,color18       ;fill in later #
          CPMOVEDC   $000,color19       ;fill in later #
          CPMOVEDC   $000,color20       ;fill in later #
          CPMOVEDC   $000,color21       ;fill in later #
          CPMOVEDC   $000,color22       ;fill in later #
          CPMOVEDC   $000,color23       ;fill in later #
          CPMOVEDC   $000,color24       ;fill in later #
          CPMOVEDC   $000,color25       ;fill in later #
          CPMOVEDC   $000,color26       ;fill in later #
          CPMOVEDC   $000,color27       ;fill in later #
          CPMOVEDC   $000,color28       ;fill in later #
          CPMOVEDC   $000,color29       ;fill in later #
          CPMOVEDC   $000,color30       ;fill in later #
          CPMOVEDC   $000,color31       ;fill in later #

* fill all these colors in later #
	AAColorPalette	1,1
	AAColorPalette	1,0
	AAColorPalette	2,1
	AAColorPalette	2,0
	AAColorPalette	3,1
	AAColorPalette	3,0
	AAColorPalette	4,1
	AAColorPalette	4,0
	AAColorPalette	5,1
	AAColorPalette	5,0
	AAColorPalette	6,1
	AAColorPalette	6,0

TFOS3Ib: EQU	*+2+(16+1)*4-TwoFieldInterface1
	AAColorPalette	7,1	;colors 17-32 = sprite colors
	AAColorPalette	7,0	;colors 17-32 = sprite colors

       	CPMOVEDC	0,copjmp1	;jump to copper list body

* MAINI1 -------------------------
TFOS1I:	EQU	*-TwoFieldInterface1
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start

TwoFI_ISPR	EQU	*-TwoFieldInterface1
TFOS2I:	EQU	*+2-TwoFieldInterface1
	CPMOVEDC	0,spr0pth	;fill in later
	CPMOVEDC	0,spr0ptl	;fill in later
	CPMOVEDC	0,spr1pth	;fill in later
	CPMOVEDC	0,spr1ptl	;fill in later

TwoFI_IBODY	EQU	*-TwoFieldInterface1
TFOS3I:	EQU	*+2-TwoFieldInterface1
	CPMOVEDC	$0,cop1lch	;->next field, fill in later 
        CPMOVEDC	$0,cop1lcl	;

	AAddfstop	AA16fetchStop
        CPMOVEDC	AAvalue,ddfstop
	
	AAbplcon1	AAscroll,AAscroll
        CPMOVEDC	AAvalue,bplcon1   	;scroll (put bm under sprite)

        CPMOVEDC   	AA16modulo,bpl1mod

TFOS4I:	EQU	*+2-TwoFieldInterface1
          CPMOVEDC   $0000,spr2pth       ;fill in later
          CPMOVEDC   $0000,spr2ptl       ;fill in later
          CPMOVEDC   $0000,spr3pth       ;fill in later
          CPMOVEDC   $0000,spr3ptl       ;fill in later
          CPMOVEDC   $0000,spr4pth       ;fill in later
          CPMOVEDC   $0000,spr4ptl       ;fill in later
          CPMOVEDC   $0000,spr5pth       ;fill in later
          CPMOVEDC   $0000,spr5ptl       ;fill in later
          CPMOVEDC   $0000,spr6pth       ;fill in later
          CPMOVEDC   $0000,spr6ptl       ;fill in later
          CPMOVEDC   $0000,spr7pth       ;fill in later
          CPMOVEDC   $0000,spr7ptl       ;fill in later

	AAbplcon3	0,1		;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

TFOS5I:	EQU	*+2-TwoFieldInterface1
		
        CPMOVEDC	BLANKGRAY,color00	;dark gray, fill in later #
	AAbplcon3	0,0		;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3
        CPMOVEDC	$000,color00	;fill in later #
	
TFOS6I:	EQU	*+2-TwoFieldInterface1
 	CPMOVEDC	$0000,bpl1pth       ;fill in later
	CPMOVEDC	$0000,bpl1ptl       ;fill in later

TFBPLCON0_1:	EQU	*+2-TwoFieldInterface1
	AAbplcon0	1,1,0,0		;depth=1, hires, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes

TFOS7I:	EQU	*-TwoFieldInterface1
	CPWAITDC	0,0		;wait for line 256??
        CPWAITDC	(21+ZT-1),19	;wait to after sprite, fill in later
	
TFBPLCON0_2:	EQU	*+2-TwoFieldInterface1
	AAbplcon0	0,1,0,0		;depth=0, hires, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes

* from here until the next WAIT, we have time for approx.
* 44 moves, but we are doing must less, so we are OK

	AAddfstop	AA768fetchStop
        CPMOVEDC	AAvalue,ddfstop

	AAbplcon1	AAbmscroll,AAbmscroll
        CPMOVEDC	AAvalue,bplcon1   	;scroll (put sprite left of bm)

TFOS8I:	EQU	*+2-TwoFieldInterface1	
        CPMOVEDC   	AA768modulo+768/8,bpl1mod
        CPMOVEDC   	AA768modulo+768/8,bpl2mod
	
TFOS9I:	EQU	*+2-TwoFieldInterface1
 	CPMOVEDC	$0000,bpl1pth       ;fill in later before install
	CPMOVEDC	$0000,bpl1ptl       ;fill in later
	CPMOVEDC	$0000,bpl2pth       ;fill in later
	CPMOVEDC	$0000,bpl2ptl       ;fill in later
	CPMOVEDC	$0000,bpl3pth       ;fill in later
	CPMOVEDC	$0000,bpl3ptl       ;fill in later
	CPMOVEDC	$0000,bpl4pth       ;fill in later
	CPMOVEDC	$0000,bpl4ptl       ;fill in later
	CPMOVEDC	$0000,bpl5pth       ;fill in later
	CPMOVEDC	$0000,bpl5ptl       ;fill in later
	CPMOVEDC	$0000,bpl6pth       ;fill in later
	CPMOVEDC	$0000,bpl6ptl       ;fill in later
	CPMOVEDC	$0000,bpl7pth       ;fill in later
	CPMOVEDC	$0000,bpl7ptl       ;fill in later
	CPMOVEDC	$0000,bpl8pth       ;fill in later
	CPMOVEDC	$0000,bpl8ptl       ;fill in later

	AAbplcon3	0,1		;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

* copper should be around horiz position 80
* wait to end of last line of ZT
TFOS10I: EQU	*-TwoFieldInterface1 
        CPWAITDC	(21+ZT-1),112	;fill in later

TFOS11I: EQU	*+2-TwoFieldInterface1
        CPMOVEDC	0,color00	;#

	AAbplcon3	0,0		;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3
        CPMOVEDC	0,color00	;#

TFOS12I:	EQU	*-TwoFieldInterface1
	CPWAITDC	0,0		;wait for line 256??
        CPWAITDC	(21+ZT),0	;fill in later
	
TFOS13I:	EQU	*+2-TwoFieldInterface1
	CPMOVEDC	0,bplcon0	;modes & #planes=1-8 Fill in later #*

* Show interface
TFOS14I: EQU	*-TwoFieldInterface1
	CPWAITDC   0,0				;wait for line 255,15/111 ?
        CPWAITDC   (21+ZT+I-1),0		;wait to last line of interface

;;	CPMOVEDC   ((408>>1)&$fc),ddfstop  	;need to help abort 4 planes of dma NOT NEEDED ON AA MACHINE

	AAbplcon3	0,1		;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

TFOS15I:	EQU	*-TwoFieldInterface1
        CPWAITDC	(21+ZT+I-1),109		;wait to near end of line, fill in later
						;can't be >109 or else sprite DMA will mess up
						;need time for 7 moves + wait
TFBPLCON0_3:	EQU	*+2-TwoFieldInterface1
	AAbplcon0	0,1,0,0		;depth=0, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes					;need time for 7 moves + wait	

TFOS16I:	EQU	*+2-TwoFieldInterface1
        CPMOVEDC	0,color00	;$842 dark gray (not black), border #*
	AAbplcon3	0,0		;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3
        CPMOVEDC	0,color00	;low contrast write #*

	AAddfstop	AA16fetchStop
        CPMOVEDC	AAvalue,ddfstop
	
	AAbplcon1	AAscroll,AAscroll
        CPMOVEDC	AAvalue,bplcon1   	;scroll (put bm under sprite)

        CPMOVEDC   	AA16modulo,bpl1mod

TFOS17I:	EQU	*+2-TwoFieldInterface1
 	CPMOVEDC	$0000,bpl1pth       ;fill in later
	CPMOVEDC	$0000,bpl1ptl       ;fill in later

TFOS18I:	EQU	*-TwoFieldInterface1
	CPWAITDC   0,0			    ;wait for line 256??
	CPWAITDC   (21+ZT+I),0		    ;fill in later

TFBPLCON0_4:	EQU	*+2-TwoFieldInterface1
	AAbplcon0	1,1,0,0		;depth=1, hires, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes

	CPENDDC			;wait to bottom of screen

* MAINII1 -------------------------
TFOS1II:	EQU	*-TwoFieldInterface1
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start

TwoFI_IISPR	EQU	*-TwoFieldInterface1
TFOS2II:	EQU	*+2-TwoFieldInterface1
	CPMOVEDC	0,spr0pth	;fill in later
	CPMOVEDC	0,spr0ptl	;fill in later
	CPMOVEDC	0,spr1pth	;fill in later
	CPMOVEDC	0,spr1ptl	;fill in later

TwoFI_IIBODY	EQU	*-TwoFieldInterface1
TFOS3II:	EQU	*+2-TwoFieldInterface1
	CPMOVEDC	$0,cop1lch	;->next field, fill in later 
        CPMOVEDC	$0,cop1lcl	;

	AAddfstop	AA16fetchStop
        CPMOVEDC	AAvalue,ddfstop
	
	AAbplcon1	AAscroll,AAscroll
        CPMOVEDC	AAvalue,bplcon1   	;scroll (put bm under sprite)

        CPMOVEDC   	AA16modulo,bpl1mod

TFOS4II:	EQU	*+2-TwoFieldInterface1
          CPMOVEDC   $0000,spr2pth       ;fill in later
          CPMOVEDC   $0000,spr2ptl       ;fill in later
          CPMOVEDC   $0000,spr3pth       ;fill in later
          CPMOVEDC   $0000,spr3ptl       ;fill in later
          CPMOVEDC   $0000,spr4pth       ;fill in later
          CPMOVEDC   $0000,spr4ptl       ;fill in later
          CPMOVEDC   $0000,spr5pth       ;fill in later
          CPMOVEDC   $0000,spr5ptl       ;fill in later
          CPMOVEDC   $0000,spr6pth       ;fill in later
          CPMOVEDC   $0000,spr6ptl       ;fill in later
          CPMOVEDC   $0000,spr7pth       ;fill in later
          CPMOVEDC   $0000,spr7ptl       ;fill in later

	AAbplcon3	0,1		;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

TFOS5II:	EQU	*+2-TwoFieldInterface1
		
        CPMOVEDC	BLANKGRAY,color00	;dark gray, fill in later #
	AAbplcon3	0,0		;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3
        CPMOVEDC	$000,color00	;fill in later #
	
TFOS6II:	EQU	*+2-TwoFieldInterface1
 	CPMOVEDC	$0000,bpl1pth       ;fill in later
	CPMOVEDC	$0000,bpl1ptl       ;fill in later

TFBPLCON0_5:	EQU	*+2-TwoFieldInterface1
	AAbplcon0	1,1,0,0		;depth=1, hires, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes

TFOS7II:	EQU	*-TwoFieldInterface1
	CPWAITDC	0,0		;wait for line 256??
        CPWAITDC	(21+ZT-1),19	;wait to after sprite, fill in later
	
TFBPLCON0_6:	EQU	*+2-TwoFieldInterface1
	AAbplcon0	0,1,0,0		;depth=0, hires, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes

* from here until the next WAIT, we have time for approx.
* 44 moves, but we are doing must less, so we are OK

	AAddfstop	AA768fetchStop
        CPMOVEDC	AAvalue,ddfstop

	AAbplcon1	AAbmscroll,AAbmscroll
        CPMOVEDC	AAvalue,bplcon1   	;scroll (put sprite left of bm)

TFOS8II:	EQU	*+2-TwoFieldInterface1	
        CPMOVEDC   	AA768modulo+768/8,bpl1mod
        CPMOVEDC   	AA768modulo+768/8,bpl2mod
	
TFOS9II:	EQU	*+2-TwoFieldInterface1
 	CPMOVEDC	$0000,bpl1pth       ;fill in later before install
	CPMOVEDC	$0000,bpl1ptl       ;fill in later
	CPMOVEDC	$0000,bpl2pth       ;fill in later
	CPMOVEDC	$0000,bpl2ptl       ;fill in later
	CPMOVEDC	$0000,bpl3pth       ;fill in later
	CPMOVEDC	$0000,bpl3ptl       ;fill in later
	CPMOVEDC	$0000,bpl4pth       ;fill in later
	CPMOVEDC	$0000,bpl4ptl       ;fill in later
	CPMOVEDC	$0000,bpl5pth       ;fill in later
	CPMOVEDC	$0000,bpl5ptl       ;fill in later
	CPMOVEDC	$0000,bpl6pth       ;fill in later
	CPMOVEDC	$0000,bpl6ptl       ;fill in later
	CPMOVEDC	$0000,bpl7pth       ;fill in later
	CPMOVEDC	$0000,bpl7ptl       ;fill in later
	CPMOVEDC	$0000,bpl8pth       ;fill in later
	CPMOVEDC	$0000,bpl8ptl       ;fill in later

	AAbplcon3	0,1		;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

* copper should be around horiz position 80
* wait to end of last line of ZT
TFOS10II: EQU	*-TwoFieldInterface1 
        CPWAITDC	(21+ZT-1),112	;fill in later

TFOS11II: EQU	*+2-TwoFieldInterface1
        CPMOVEDC	0,color00	;#

	AAbplcon3	0,0		;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3
        CPMOVEDC	0,color00	;#

TFOS12II:	EQU	*-TwoFieldInterface1
	CPWAITDC	0,0		;wait for line 256??
        CPWAITDC	(21+ZT),0	;fill in later
	
TFOS13II:	EQU	*+2-TwoFieldInterface1
	CPMOVEDC	0,bplcon0	;modes & #planes=1-8 Fill in later #*

* Show interface
TFOS14II: EQU	*-TwoFieldInterface1
	CPWAITDC   0,0				;wait for line 255,15/111 ?
        CPWAITDC   (21+ZT+I-1),0		;wait to last line of interface

;;	CPMOVEDC   ((408>>1)&$fc),ddfstop  	;need to help abort 4 planes of dma NOT NEEDED ON AA MACHINE

	AAbplcon3	0,1		;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

TFOS15II:	EQU	*-TwoFieldInterface1
        CPWAITDC	(21+ZT+I-1),109		;wait to near end of line, fill in later
						;can't be >109 or else sprite DMA will mess up
						;need time for 7 moves + wait	AAbplcon0	0,1,0,0		;depth=0, HiRes, non-HAM, no ERSY
TFBPLCON0_7:	EQU	*+2-TwoFieldInterface1
	AAbplcon0	0,1,0,0		;depth=0, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes					;need time for 7 moves + wait	

TFOS16II:	EQU	*+2-TwoFieldInterface1
        CPMOVEDC	0,color00	;$842 dark gray (not black), border #*
	AAbplcon3	0,0		;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3
        CPMOVEDC	0,color00	;low contrast write #*

