SafeWriteCopList2:
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start
	CPMOVEDC	0,spr0pth	;fill in later #
	CPMOVEDC	0,spr0ptl	;fill in later #
	CPMOVEDC	0,spr1pth	;fill in later #
	CPMOVEDC	0,spr1ptl	;fill in later #

	CPMOVEDC	0,cop1lch	;->after Write copper list body, fill in later #
        CPMOVEDC	0,cop1lcl	;->after Wrtte copper list body, fill in later #

	AAdiwstrt	AAwindowStartX,AAwindowStartY					
        CPMOVEDC	AAvalue,diwstrt

        CPMOVEDC	0,diwstop		;#
	CPMOVEDC	0,diwhigh		;#

	AAddfstrt	AAfetchStart		;same for all displays
        CPMOVEDC	AAvalue,ddfstrt

        CPMOVEDC   	AAbplcon2,bplcon2	;sprite priority over bm
	CPMOVEDC	AAbplcon4,bplcon4	;sprite colors = 240-255

	AAbplcon3	0,1	;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

	CPMOVEDC   BLANKGRAY,color00	;color 0 is $842 dark gray (not black), border??? #
        CPMOVEDC   BLANKGRAY,color01  
        CPMOVEDC   BLANKGRAY,color02  
        CPMOVEDC   BLANKGRAY,color03  
        CPMOVEDC   BLANKGRAY,color04  
        CPMOVEDC   BLANKGRAY,color05  
        CPMOVEDC   BLANKGRAY,color06  
        CPMOVEDC   BLANKGRAY,color07  
        CPMOVEDC   BLANKGRAY,color08  
        CPMOVEDC   BLANKGRAY,color09  
        CPMOVEDC   BLANKGRAY,color10  
        CPMOVEDC   BLANKGRAY,color11  
        CPMOVEDC   BLANKGRAY,color12  
        CPMOVEDC   BLANKGRAY,color13  
        CPMOVEDC   BLANKGRAY,color14  
        CPMOVEDC   BLANKGRAY,color15

	CPMOVEDC      0,color16	;fill in later #
	CPMOVEDC      0,color17	;fill in later #
	CPMOVEDC      0,color18	;fill in later #
	CPMOVEDC      0,color19	;fill in later #
	CPMOVEDC      0,color20	;fill in later #
	CPMOVEDC      0,color21	;fill in later #
	CPMOVEDC      0,color22	;fill in later #
	CPMOVEDC      0,color23	;fill in later #
	CPMOVEDC      0,color24	;fill in later #
	CPMOVEDC      0,color25	;fill in later #
	CPMOVEDC      0,color26	;fill in later #
	CPMOVEDC      0,color27	;fill in later #
	CPMOVEDC      0,color28	;fill in later #
	CPMOVEDC      0,color29	;fill in later #
	CPMOVEDC      0,color30	;fill in later #
	CPMOVEDC      0,color31	;fill in later #
  
	AAbplcon3	0,0	;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

        CPMOVEDC   $000,color00   G0=0 R0=0 B2=0 R2=0	;color 0 is $842 dark gray (not black), border??? #
        CPMOVEDC   $400,color01   G0=0 R0=0 B2=0 R2=1
        CPMOVEDC   $004,color02   G0=0 R0=0 B2=1 R2=0
        CPMOVEDC   $404,color03   G0=0 R0=0 B2=1 R2=1
        CPMOVEDC   $100,color04   G0=0 R0=1 B2=0 R2=0
        CPMOVEDC   $500,color05   G0=0 R0=1 B2=0 R2=1
        CPMOVEDC   $104,color06   G0=0 R0=1 B2=1 R2=0
        CPMOVEDC   $504,color07   G0=0 R0=1 B2=1 R2=1
        CPMOVEDC   $010,color08   G0=1 R0=0 B2=0 R2=0
        CPMOVEDC   $410,color09   G0=1 R0=0 B2=0 R2=1
        CPMOVEDC   $014,color10   G0=1 R0=0 B2=1 R2=0
        CPMOVEDC   $414,color11   G0=1 R0=0 B2=1 R2=1
        CPMOVEDC   $110,color12   G0=1 R0=1 B2=0 R2=0
        CPMOVEDC   $510,color13   G0=1 R0=1 B2=0 R2=1
        CPMOVEDC   $114,color14   G0=1 R0=1 B2=1 R2=0
        CPMOVEDC   $514,color15   G0=1 R0=1 B2=1 R2=1

	CPMOVEDC      0,color16	;fill in later #
	CPMOVEDC      0,color17	;fill in later #
	CPMOVEDC      0,color18	;fill in later #
	CPMOVEDC      0,color19	;fill in later #
	CPMOVEDC      0,color20	;fill in later #
	CPMOVEDC      0,color21	;fill in later #
	CPMOVEDC      0,color22	;fill in later #
	CPMOVEDC      0,color23	;fill in later #
	CPMOVEDC      0,color24	;fill in later #
	CPMOVEDC      0,color25	;fill in later #
	CPMOVEDC      0,color26	;fill in later #
	CPMOVEDC      0,color27	;fill in later #
	CPMOVEDC      0,color28	;fill in later #
	CPMOVEDC      0,color29	;fill in later #
	CPMOVEDC      0,color30	;fill in later #
	CPMOVEDC      0,color31	;fill in later #

	AAColorPalette	1,1	;#
	AAColorPalette	1,0	;#
	AAColorPalette	2,1	;#
	AAColorPalette	2,0	;#
	AAColorPalette	3,1	;#
	AAColorPalette	3,0	;#
	AAColorPalette	4,1	;#
	AAColorPalette	4,0	;#
	AAColorPalette	5,1	;#
	AAColorPalette	5,0	;#
	AAColorPalette	6,1	;#
	AAColorPalette	6,0	;#
	AAColorPalette	7,1	;colors 17-32 = sprite colors #
	AAColorPalette	7,0	;colors 17-32 = sprite colors #

	AAddfstop	AA16fetchStop
	CPMOVEDC	AAvalue,ddfstop

	AAbplcon1	AAscroll,AAscroll
        CPMOVEDC	AAvalue,bplcon1   	;scroll (put it under sprite)

        CPMOVEDC	AA16modulo,bpl1mod		

 	CPMOVEDC	$0000,bpl1pth       ;fill in later #
	CPMOVEDC	$0000,bpl1ptl       ;fill in later #

SWSPR2	EQU	*+2
          CPMOVEDC   $0000,spr2pth       ;fill in later #
          CPMOVEDC   $0000,spr2ptl       ;fill in later #
          CPMOVEDC   $0000,spr3pth       ;fill in later #
          CPMOVEDC   $0000,spr3ptl       ;fill in later #
          CPMOVEDC   $0000,spr4pth       ;fill in later #
          CPMOVEDC   $0000,spr4ptl       ;fill in later #
          CPMOVEDC   $0000,spr5pth       ;fill in later #
          CPMOVEDC   $0000,spr5ptl       ;fill in later #
          CPMOVEDC   $0000,spr6pth       ;fill in later #
          CPMOVEDC   $0000,spr6ptl       ;fill in later #
          CPMOVEDC   $0000,spr7pth       ;fill in later #
          CPMOVEDC   $0000,spr7ptl       ;fill in later #

	AAbplcon0	1,1,0,0		;depth=1, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes

	CPMOVEDC	AAfetchMode,fmode
	
* do headers HDR3, HDR1, HDR2, HDR0
        CPWAITDC	(21+3),19	;wait to after sprite

*----------------

	AAbplcon0	0,1,0,0		;depth=0, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes

	AAbplcon3	0,1	;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3
        CPMOVEDC	BLANKGRAY,color00
	AAbplcon3	0,0	;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3
        CPMOVEDC	$000,color00	;low contrast write

	AAddfstop	AA768fetchStop
        CPMOVEDC	AAvalue,ddfstop

	AAbplcon1	AAbmscroll,AAbmscroll
        CPMOVEDC	AAvalue,bplcon1   	;scroll (put sprite left of bm)

	CPMOVEDC	0,bpl1mod	;-96 for sync write, -4 for 736 wide, 0 for 768 wide, fill in later #
	CPMOVEDC	0,bpl2mod	; #
	
 	CPMOVEDC	$0000,bpl1pth       ;fill in later #
	CPMOVEDC	$0000,bpl1ptl       ;fill in later #
	CPMOVEDC	$0000,bpl2pth       ;fill in later #
	CPMOVEDC	$0000,bpl2ptl       ;fill in later #
	CPMOVEDC	$0000,bpl3pth       ;fill in later #
	CPMOVEDC	$0000,bpl3ptl       ;fill in later #
	CPMOVEDC	$0000,bpl4pth       ;fill in later #
	CPMOVEDC	$0000,bpl4ptl       ;fill in later #

        CPWAITDC	(21+4),0

*----------------
	
	AAbplcon0	4,1,0,0		;depth=4, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes

* do Top write
	CPWAITDC	0,0		;wait for line 256?? #
	CPWAITDC   (21+4+DT),0		;fill in later #

	AAddfstop	AA16fetchStop
	CPMOVEDC	AAvalue,ddfstop	;wait to after sprite

* do HDR0
	CPWAITDC   (21+4+DT),19		;fill in later #	;must be >=17

	AAbplcon0	0,1,0,0		;depth=0, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes
	
*----------------
	AAbplcon3	0,1		;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

        CPMOVEDC	0,color00	;$842 dark gray (not black), border #
	AAbplcon3	0,0		;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3
        CPMOVEDC	0,color00	;low contrast write #

	AAddfstop	AA16fetchStop
	CPMOVEDC	AAvalue,ddfstop

	AAbplcon1	AAscroll,AAscroll
        CPMOVEDC	AAvalue,bplcon1   	;scroll (put it under sprite)

        CPMOVEDC	AA16modulo,bpl1mod		

 	CPMOVEDC	$0000,bpl1pth       ;fill in later #
	CPMOVEDC	$0000,bpl1ptl       ;fill in later #

	CPWAITDC   (21+4+DT+1),0	;fill in later #

	AAbplcon0	1,1,0,0		;depth=1, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes

* do headers HDR2, HDR1, HDR3

* This palette stuff used to be after the WAIT to (21+4+DT+3),19 .
* But to have enough time, I needed to put it earlier.

	AAbplcon3	0,1		;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

	CPMOVEDC	$0,color01	;#
	CPMOVEDC	$0,color02	;#
	CPMOVEDC	$0,color03	;#
	CPMOVEDC	$0,color04	;# 
	CPMOVEDC	$0,color05	;#
	CPMOVEDC	$0,color06	;# 
	CPMOVEDC	$0,color07	;# 
	CPMOVEDC	$0,color08	;#
	CPMOVEDC	$0,color09	;# 
	CPMOVEDC	$0,color10	;# 
	CPMOVEDC	$0,color11	;#
	CPMOVEDC	$0,color12	;# 
	CPMOVEDC	$0,color13	;# 
	CPMOVEDC	$0,color14	;#
	CPMOVEDC	$0,color15	;# 

	AAbplcon3	0,0		;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3

	CPMOVEDC	$0,color01	;#
	CPMOVEDC	$0,color02	;#
	CPMOVEDC	$0,color03	;#
	CPMOVEDC	$0,color04	;# 
	CPMOVEDC	$0,color05	;#
	CPMOVEDC	$0,color06	;# 
	CPMOVEDC	$0,color07	;# 
	CPMOVEDC	$0,color08	;#
	CPMOVEDC	$0,color09	;# 
	CPMOVEDC	$0,color10	;# 
	CPMOVEDC	$0,color11	;#
	CPMOVEDC	$0,color12	;# 
	CPMOVEDC	$0,color13	;# 
	CPMOVEDC	$0,color14	;#
	CPMOVEDC	$0,color15	;# 

	CPWAITDC	0,0		;wait for line 256?? #
        CPWAITDC	(21+4+DT+3),19	;wait to after sprite, fill in later #
	
*----------------

	AAbplcon0	0,1,0,0		;depth=0, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes

* Even if ZT=0, from here until the next WAIT, we have time for approx.
* 44 moves, but we are only attempting 20, so we are OK.

	AAddfstop	AA768fetchStop
        CPMOVEDC	AAvalue,ddfstop

	AAbplcon1	AAbmscroll,AAbmscroll
        CPMOVEDC	AAvalue,bplcon1   	;scroll (put sprite left of bm)

	CPMOVEDC	0,bpl1mod	;92 for 736 wide, 96 for 768 wide, fill in later #
	CPMOVEDC	0,bpl2mod	;#
	
 	CPMOVEDC	$0000,bpl1pth       ;fill in later before install #
	CPMOVEDC	$0000,bpl1ptl       ;fill in later #
	CPMOVEDC	$0000,bpl2pth       ;fill in later #
	CPMOVEDC	$0000,bpl2ptl       ;fill in later #
	CPMOVEDC	$0000,bpl3pth       ;fill in later #
	CPMOVEDC	$0000,bpl3ptl       ;fill in later #
	CPMOVEDC	$0000,bpl4pth       ;fill in later #
	CPMOVEDC	$0000,bpl4ptl       ;fill in later #
	CPMOVEDC	$0000,bpl5pth       ;fill in later #
	CPMOVEDC	$0000,bpl5ptl       ;fill in later #
	CPMOVEDC	$0000,bpl6pth       ;fill in later #
	CPMOVEDC	$0000,bpl6ptl       ;fill in later #
	CPMOVEDC	$0000,bpl7pth       ;fill in later #
	CPMOVEDC	$0000,bpl7ptl       ;fill in later #
	CPMOVEDC	$0000,bpl8pth       ;fill in later #
	CPMOVEDC	$0000,bpl8ptl       ;fill in later #

	AAbplcon3	0,1		;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

* copper should be around horiz position 80
* wait to end of last line of ZT, or wait forever if no more.
        CPWAITDC	(21+4+DT+4+ZT-1),114 ;fill in later #

        CPMOVEDC	0,color00	;#

	AAbplcon3	0,0		;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3
        CPMOVEDC	0,color00	;#

        CPWAITDC	(21+4+DT+4+ZT),0		;fill in later #
	
*----------------

	CPMOVEDC	0,bplcon0	;modes & #planes=1-4 Fill in later #

* Show interface
	CPWAITDC	0,0			;wait to 255,15/111 ?
        CPWAITDC	(21+4+DT+4+ZT+I-1),0	;wait to last line of interface

* need to help abort 8 planes of DMA ? (NOT TRUE!)
* This seems to work with fetches >= 364.  So the standard 400 works fine.
* This was 408 on ECS chips.
;;	AAddfstop	AA768fetchStop
;;	CPMOVEDC	AAvalue,ddfstop	;wait to after sprite

	AAbplcon3	0,1		;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

        CPWAITDC	(21+4+DT+4+ZT+I-1),109	;wait to near end of line, fill in later #
						;can't be >109 or else sprite DMA will mess up
	AAbplcon0	0,1,0,0		;depth=0, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes					;need time for 7 moves + wait	

        CPMOVEDC	0,color00	;$842 dark gray (not black), border #
	AAbplcon3	0,0		;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3
        CPMOVEDC	0,color00	;low contrast write #

	AAddfstop	AA16fetchStop
	CPMOVEDC	AAvalue,ddfstop

	AAbplcon1	AAscroll,AAscroll
        CPMOVEDC	AAvalue,bplcon1   	;scroll (put it under sprite)

        CPMOVEDC	AA16modulo,bpl1mod		

 	CPMOVEDC	$0000,bpl1pth       ;fill in later #
	CPMOVEDC	$0000,bpl1ptl       ;fill in later #

	CPWAITDC   (21+4+DT+4+ZT+I+ZB),0	;fill in later	#

*----------------

	AAbplcon0	1,1,0,0		;depth=1, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes

* do headers HDR3, HDR1, HDR2, HDR0

* This palette stuff used to be after wait to (21+4+DT+4+ZT+I+ZB+3),19 .
* But I had to move it up here so it had time to work.

	AAbplcon3	0,1	;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3
	
        CPMOVEDC   BLANKGRAY,color01  
        CPMOVEDC   BLANKGRAY,color02  
        CPMOVEDC   BLANKGRAY,color03  
        CPMOVEDC   BLANKGRAY,color04  
        CPMOVEDC   BLANKGRAY,color05  
        CPMOVEDC   BLANKGRAY,color06  
        CPMOVEDC   BLANKGRAY,color07  
        CPMOVEDC   BLANKGRAY,color08  
        CPMOVEDC   BLANKGRAY,color09  
        CPMOVEDC   BLANKGRAY,color10  
        CPMOVEDC   BLANKGRAY,color11  
        CPMOVEDC   BLANKGRAY,color12  
        CPMOVEDC   BLANKGRAY,color13  
        CPMOVEDC   BLANKGRAY,color14  
        CPMOVEDC   BLANKGRAY,color15  

	AAbplcon3	0,0	;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3

        CPMOVEDC   $400,color01   G0=0 R0=0 B2=0 R2=1
        CPMOVEDC   $004,color02   G0=0 R0=0 B2=1 R2=0
        CPMOVEDC   $404,color03   G0=0 R0=0 B2=1 R2=1
        CPMOVEDC   $100,color04   G0=0 R0=1 B2=0 R2=0
        CPMOVEDC   $500,color05   G0=0 R0=1 B2=0 R2=1
        CPMOVEDC   $104,color06   G0=0 R0=1 B2=1 R2=0
        CPMOVEDC   $504,color07   G0=0 R0=1 B2=1 R2=1
        CPMOVEDC   $010,color08   G0=1 R0=0 B2=0 R2=0
        CPMOVEDC   $410,color09   G0=1 R0=0 B2=0 R2=1
        CPMOVEDC   $014,color10   G0=1 R0=0 B2=1 R2=0
        CPMOVEDC   $414,color11   G0=1 R0=0 B2=1 R2=1
        CPMOVEDC   $110,color12   G0=1 R0=1 B2=0 R2=0
        CPMOVEDC   $510,color13   G0=1 R0=1 B2=0 R2=1
        CPMOVEDC   $114,color14   G0=1 R0=1 B2=1 R2=0
        CPMOVEDC   $514,color15   G0=1 R0=1 B2=1 R2=1

	CPWAITDC	0,0	;wait to after line 256?, fill in later #
        CPWAITDC	(21+4+DT+4+ZT+I+ZB+3),19 ;>256?, wait to after sprite, fill in later #
	
	AAbplcon0	0,1,0,0		;depth=0, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes
	
	AAbplcon3	0,1	;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3
        CPMOVEDC	BLANKGRAY,color00
	AAbplcon3	0,0	;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3
        CPMOVEDC	$000,color00	;low contrast write

	AAddfstop	AA768fetchStop
        CPMOVEDC	AAvalue,ddfstop

	AAbplcon1	AAbmscroll,AAbmscroll
        CPMOVEDC	AAvalue,bplcon1   	;scroll (put sprite left of bm)

	CPMOVEDC	0,bpl1mod	;-96 for sync write, -4 for 736 wide, 0 for 768 wide, fill in later #
	CPMOVEDC	0,bpl2mod	; #
	
 	CPMOVEDC	$0000,bpl1pth       ;fill in later #
	CPMOVEDC	$0000,bpl1ptl       ;fill in later #
	CPMOVEDC	$0000,bpl2pth       ;fill in later #
	CPMOVEDC	$0000,bpl2ptl       ;fill in later #
	CPMOVEDC	$0000,bpl3pth       ;fill in later #
	CPMOVEDC	$0000,bpl3ptl       ;fill in later #
	CPMOVEDC	$0000,bpl4pth       ;fill in later #
	CPMOVEDC	$0000,bpl4ptl       ;fill in later #

        CPWAITDC	(21+4+DT+4+ZT+I+ZB+4),0	;>256?, fill in later #
	
*----------------

	AAbplcon0	4,1,0,0		;depth=4, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes

* do Bottom write
	CPWAITDC	0,0	;wait to after line 256?, fill in later #
	CPWAITDC   (21+4+DT+4+ZT+I+ZB+4+DB-256),0	;fill in later	#

	AAddfstop	AA16fetchStop
	CPMOVEDC	AAvalue,ddfstop	;wait to after sprite

* do HDR0
	CPWAITDC  (21+4+DT+4+ZT+I+ZB+4+DB-256),19	;fill in later #

	AAbplcon0	0,1,0,0		;depth=0, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes
	
	AAbplcon3	0,1		;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

        CPMOVEDC	0,color00	;# $842 dark gray (not black), border
	AAbplcon3	0,0		;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3
        CPMOVEDC	0,color00	;#

	AAddfstop	AA16fetchStop
	CPMOVEDC	AAvalue,ddfstop

	AAbplcon1	AAscroll,AAscroll
        CPMOVEDC	AAvalue,bplcon1   	;scroll (put it under sprite)

        CPMOVEDC	AA16modulo,bpl1mod		

 	CPMOVEDC	$0000,bpl1pth       ;fill in later #
	CPMOVEDC	$0000,bpl1ptl       ;fill in later #

	CPWAITDC   (21+4+DT+4+ZT+I+ZB+4+DB+1-256),0	;fill in later #

	AAbplcon0	1,1,0,0		;depth=1, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes

* do headers HDR3, HDR1, HDR2

	CPENDDC

*********************************************************
* This could be improved to animate the BM in 2-D.
* This CopList is used by ReadScanLine.
SimpleBMCopList1:
SSBMintreq1	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start

SBM1	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	0,spr0pth	;fill in later #
	CPMOVEDC	0,spr0ptl	;fill in later #
	CPMOVEDC	0,spr1pth	;fill in later #
	CPMOVEDC	0,spr1ptl	;fill in later #

* window start & stops are put her to prevent premature starts
* These will be filled in later!
SBM1b	EQU	*+2-SimpleBMCopList1
	AAdiwstrt	AAwindowStartX,AAwindowStartY					
        CPMOVEDC	AAvalue,diwstrt

	AAdiwstop	AAwindowStopX,AAwindowStopY
        CPMOVEDC	AAvalue,diwstop

	AAdiwhigh	AAwindowStartX,AAwindowStopX,AAwindowStartY,AAwindowStopY
	CPMOVEDC	AAvalue,diwhigh

SBM_LATCH	EQU	*-SimpleBMCopList1
SBM2	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	$0,cop1lch	;->AVE copper list body, fill in later #
        CPMOVEDC	$0,cop1lcl	;->AVE copper list body, fill in later #

*---------
SBM7	EQU	*+6-SimpleBMCopList1
	AAColorPalette	0,1
	AAColorPalette	0,0
	AAColorPalette	1,1
	AAColorPalette	1,0
	AAColorPalette	2,1
	AAColorPalette	2,0
	AAColorPalette	3,1
	AAColorPalette	3,0
	AAColorPalette	4,1
	AAColorPalette	4,0
	AAColorPalette	5,1
	AAColorPalette	5,0
	AAColorPalette	6,1
	AAColorPalette	6,0

SBM3	EQU	*+6+(16*4)-SimpleBMCopList1	;->color 16
	AAColorPalette	7,1	;colors 17-32 = sprite colors
	AAColorPalette	7,0	;colors 17-32 = sprite colors

SBM8	EQU	*+2-SimpleBMCopList1
        CPMOVEDC	0,bplcon1	;fill in later
        CPMOVEDC	0,bpl1mod	;fill in later
        CPMOVEDC	0,bpl2mod	;fill in later
        CPMOVEDC	0,ddfstrt	;fill in later
        CPMOVEDC	0,ddfstop	;fill in later

SBM8b	EQU	*+2-SimpleBMCopList1
        CPMOVEDC   	AAbplcon2,bplcon2	;sprite priority over bm
						;ZDCTEN bit filled in later#

	CPMOVEDC	AAbplcon4,bplcon4	;sprite colors = 240-255

       	CPMOVEDC	0,copjmp1	;jump to copper list body
*---------
SBMMain1:
SSBMintreq2	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq        ;copper interrupt = flag start

SBMSPR	EQU	*-SimpleBMCopList1
	CPMOVEDC	0,spr0pth	;fill in later #
	CPMOVEDC	0,spr0ptl	;fill in later #
	CPMOVEDC	0,spr1pth	;fill in later #
	CPMOVEDC	0,spr1ptl	;fill in later #

*---------
SBM_IBODY	EQU	*-SimpleBMCopList1
SBMBody1:
SBM4	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	$0,cop1lch	;->AVE copper list body, fill in later #
        CPMOVEDC	$0,cop1lcl	;->AVE copper list body, fill in later #

SBM5	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	$0,bpl1pth	;fill in later #
	CPMOVEDC	$0,bpl1ptl	;fill in later #
	CPMOVEDC	$0,bpl2pth	;fill in later #
	CPMOVEDC	$0,bpl2ptl	;fill in later #
	CPMOVEDC	$0,bpl3pth	;fill in later #
	CPMOVEDC	$0,bpl3ptl	;fill in later #
	CPMOVEDC	$0,bpl4pth	;fill in later #
	CPMOVEDC	$0,bpl4ptl	;fill in later #
	CPMOVEDC	$0,bpl5pth	;fill in later #
	CPMOVEDC	$0,bpl5ptl	;fill in later #
	CPMOVEDC	$0,bpl6pth	;fill in later #
	CPMOVEDC	$0,bpl6ptl	;fill in later #
	CPMOVEDC	$0,bpl7pth	;fill in later #
	CPMOVEDC	$0,bpl7ptl	;fill in later #
	CPMOVEDC	$0,bpl8pth	;fill in later #
	CPMOVEDC	$0,bpl8ptl	;fill in later #

SBM6	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	0,spr2pth       ;fill in later #
	CPMOVEDC   	0,spr2ptl       ;fill in later #
	CPMOVEDC	0,spr3pth       ;fill in later #
	CPMOVEDC	0,spr3ptl       ;fill in later #
	CPMOVEDC	0,spr4pth       ;fill in later #
	CPMOVEDC	0,spr4ptl       ;fill in later #
	CPMOVEDC	0,spr5pth       ;fill in later #
	CPMOVEDC	0,spr5ptl       ;fill in later #
	CPMOVEDC	0,spr6pth       ;fill in later #
	CPMOVEDC	0,spr6ptl       ;fill in later #
	CPMOVEDC	0,spr7pth       ;fill in later #
	CPMOVEDC	0,spr7ptl       ;fill in later #

	CPMOVEDC	AAfetchMode,fmode
	
SBMBPLCON0_1	EQU	*+2-SimpleBMCopList1
* use Lowres?
	AAbplcon0	0,1,0,0			;depth=0, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue!1,bplcon0	;modes & #planes + enable bplcon3 special bits

SBM9	EQU	*-SimpleBMCopList1
	CPWAITDC	0,0	;wait to after 255?,    fill in later
	CPWAITDC	0,0	;wait to top of effect, fill in later

	CPMOVEDC	0,bplcon0	;modes & #planes=1-8, fill in later #

	CPWAITDC	0,0	;wait to after 255?,    fill in later
	CPWAITDC	0,0	;wait to top of effect, fill in later

SBMBPLCON0_2	EQU	*+2-SimpleBMCopList1
* use Lowres?
	AAbplcon0	0,1,0,0			;depth=0, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue!1,bplcon0	;modes & #planes + enable bplcon3 special bits

          CPENDDC

*---------
SBMMain1b:
SSBMintreq3	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq        ;copper interrupt = flag start

SBMSPRb	EQU	*-SimpleBMCopList1
	CPMOVEDC	0,spr0pth	;fill in later #
	CPMOVEDC	0,spr0ptl	;fill in later #
	CPMOVEDC	0,spr1pth	;fill in later #
	CPMOVEDC	0,spr1ptl	;fill in later #

*---------
SBM_IIBODY	EQU	*-SimpleBMCopList1
SBMBody1b:
SBM4b	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	$0,cop1lch	;->AVE copper list body, fill in later #
        CPMOVEDC	$0,cop1lcl	;->AVE copper list body, fill in later #

SBM5b	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	$0,bpl1pth	;fill in later #
	CPMOVEDC	$0,bpl1ptl	;fill in later #
	CPMOVEDC	$0,bpl2pth	;fill in later #
	CPMOVEDC	$0,bpl2ptl	;fill in later #
	CPMOVEDC	$0,bpl3pth	;fill in later #
	CPMOVEDC	$0,bpl3ptl	;fill in later #
	CPMOVEDC	$0,bpl4pth	;fill in later #
	CPMOVEDC	$0,bpl4ptl	;fill in later #
	CPMOVEDC	$0,bpl5pth	;fill in later #
	CPMOVEDC	$0,bpl5ptl	;fill in later #
	CPMOVEDC	$0,bpl6pth	;fill in later #
	CPMOVEDC	$0,bpl6ptl	;fill in later #
	CPMOVEDC	$0,bpl7pth	;fill in later #
	CPMOVEDC	$0,bpl7ptl	;fill in later #
	CPMOVEDC	$0,bpl8pth	;fill in later #
	CPMOVEDC	$0,bpl8ptl	;fill in later #

SBM6b	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	0,spr2pth       ;fill in later #
	CPMOVEDC   	0,spr2ptl       ;fill in later #
	CPMOVEDC	0,spr3pth       ;fill in later #
	CPMOVEDC	0,spr3ptl       ;fill in later #
	CPMOVEDC	0,spr4pth       ;fill in later #
	CPMOVEDC	0,spr4ptl       ;fill in later #
	CPMOVEDC	0,spr5pth       ;fill in later #
	CPMOVEDC	0,spr5ptl       ;fill in later #
	CPMOVEDC	0,spr6pth       ;fill in later #
	CPMOVEDC	0,spr6ptl       ;fill in later #
	CPMOVEDC	0,spr7pth       ;fill in later #
	CPMOVEDC	0,spr7ptl       ;fill in later #

	CPMOVEDC	AAfetchMode,fmode
	
SBMBPLCON0_1b	EQU	*+2-SimpleBMCopList1
* use Lowres?
	AAbplcon0	0,1,0,0			;depth=0, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue!1,bplcon0	;modes & #planes + enable bplcon3 special bits

SBM9b	EQU	*-SimpleBMCopList1
	CPWAITDC	0,0	;wait to after 255?,    fill in later
	CPWAITDC	0,0	;wait to top of effect, fill in later

	CPMOVEDC	0,bplcon0	;modes & #planes=1-8, fill in later #

	CPWAITDC	0,0	;wait to after 255?,    fill in later
	CPWAITDC	0,0	;wait to top of effect, fill in later

SBMBPLCON0_2b	EQU	*+2-SimpleBMCopList1
* use Lowres?
	AAbplcon0	0,1,0,0			;depth=0, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue!1,bplcon0	;modes & #planes + enable bplcon3 special bits

          CPENDDC

