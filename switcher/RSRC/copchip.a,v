	include	'assembler.i'
;;	include	"RAM:conditional.i"

	include	"exec/types.i"
	include	"hardware/custom.i"
	include "hardware/intbits.i"

	include	"macros.i"
	include	"custom.i"
	include "copper.i"
	include "vthand.i"
	include "cop.i"


	IFND	AACHIPS
NUMINTERFACEPLANES	SET	2
	ENDC
	IFD	AACHIPS
NUMINTERFACEPLANES	SET	6
	ENDC

* WHY WHERE THESE ANALOG COPPERLISTS DOUBLE BUFFERED ????????????
	XDATA	AVESPR,AVEISPR1,AVEISPR2
	XDATA	AVECopList1,AVECL1,AVECopListMain1,AVECopListBody1
	XDATA	AVEInterface1,AVEI1,AVEInterfaceBody1
	XDATA	AVEInterfaceMainI1,AVEInterfaceMainII1
	XDATA	AVE_LATCH,AVE_BODY,AVE_COLOR
	XDATA	AVEI_LATCH,AVEI_PALETTE
	XDATA	AVEI_IBODY,AVEI_IIBODY
	XDATA	AVEI_I,AVEI_II
	XDATA	AVEintreq1,AVEintreq2

	XDATA	AVECopList2,AVECL2,AVECopListMain2,AVECopListBody2
	XDATA	AVEInterface2,AVEI2,AVEInterfaceBody2
	XDATA	AVEInterfaceMainI2,AVEInterfaceMainII2

	XDATA	ReadCopList1,ReadCopList2,ReadLCH1,ReadLCH2
	XDATA	ReadMain1,ReadMain2,ReadBpl1,ReadBpl2
	XDATA	ReadCopperLists,ReadCopListPlanes
		
	XDATA	FieldWriteCopList1,FieldWriteCopList2,FWSPR1,FWSPR2	

	XDATA	SystemLink,FWMOD,FWPLANES,FWSCROLL

	XDATA	SafeWriteCopList1,SafeWriteCopList2,SWSPR1,SWSPR2
	XDATA	SWOS1,SWOS2,SWOS2b,SWOS2c
	XDATA	SWOS3,SWOS4,SWOS5,SWOS6
	XDATA	SWOS7,SWOS7b,SWOS8,SWOS9
	XDATA	SWOS10,SWOS10b,SWOS11,SWOS12
	XDATA	SWOS12b,SWOS12c,SWOS13,SWOS13b
	XDATA	SWOS13c,SWOS14,SWOS14b,SWOS15
	XDATA	SWOS16,SWOS17,SWOS18,SWOS19
	XDATA	SWOS20,SWOS21,SWOS22,SWOS22b
	XDATA	SWOS23,SWOS24
	
	IFD	AACHIPS
	XDATA	SWOS2d,SWOS3a,SWOS3b,SWOS17b
	XDATA	SWOS16b,SWOS17c
	ENDC

	XDATA	TwoFieldInterface1,TwoFieldInterface2
	XDATA	TwoFI_LATCH,TwoFI_IBODY,TwoFI_IIBODY
	XDATA	TwoFI_ISPR,TwoFI_IISPR
	XDATA	TFOS1,TFOS2,TFOS3
	XDATA	TFOS1I,TFOS2I,TFOS3I,TFOS4I
	XDATA	TFOS5I,TFOS6I,TFOS7I,TFOS8I
	XDATA	TFOS9I,TFOS10I,TFOS11I,TFOS12I
	XDATA	TFOS13I,TFOS14I,TFOS15I,TFOS16I
	XDATA	TFOS17I,TFOS18I
	XDATA	TFOS1II,TFOS2II,TFOS3II,TFOS4II
	XDATA	TFOS5II,TFOS6II,TFOS7II,TFOS8II
	XDATA	TFOS9II,TFOS10II,TFOS11II,TFOS12II
	XDATA	TFOS13II,TFOS14II,TFOS15II,TFOS16II
	XDATA	TFOS17II,TFOS18II

	IFD	AACHIPS
	XDATA	TFOS3Ib
	ENDC

	XDATA	SimpleBMCopList1,SimpleBMCopList2,SBM1,SBM2
	XDATA	SBMSPR,SBM4,SBM5,SBM6
	XDATA	SBM7,SBM8,SBM9,SBM3,SBM8b
	XDATA	SBMMain1,SBMBody1,SBMMain2,SBMBody2

	XDATA	SBMSPRb,SBM4b,SBM5b,SBM6b
	XDATA	SBM9b
	XDATA	SBMMain1b,SBMBody1b,SBMMain2b,SBMBody2b
	XDATA	SBM_LATCH,SBM_IBODY,SBM_IIBODY
	XDATA	SSBMintreq1,SSBMintreq2,SSBMintreq3

	IFD	AACHIPS
	XDATA	SBM1b
	ENDC

	XDATA	SpriteData,Sprite0LiveA,Sprite0LiveB
	XDATA	Sprite0FreezeA,Sprite0FreezeB

* DVESprite0A,DVESprite0B
	XDATA	DVESprite1A,DVESprite1B
	XDATA	VTSprite0A,VTSprite0B,VTSprite1A,VTSprite1B
	XDATA	ELHSprite0A,ELHSprite0B,ELHSprite1A,ELHSprite1B
	XDATA	SpriteNull,Sprite1Linear,NarrowBMNull

	XDATA	AVEBPLCON0_1,AVEBPLCON0_2
	XDATA	AVEI_IBPLCON0_1,AVEI_IBPLCON0_2,AVEI_IIBPLCON0_1,AVEI_IIBPLCON0_2
	XDATA	AVEI_IBPLCON0_1b,AVEI_IBPLCON0_2b,AVEI_IIBPLCON0_1b,AVEI_IIBPLCON0_2b
	XDATA	ReadBPLCON0_1,ReadBPLCON0_2,FWBPLCON0_1,FWBPLCON0_2
	XDATA	SWBPLCON0_1,SWBPLCON0_2,SWBPLCON0_3,SWBPLCON0_4
	XDATA	SWBPLCON0_5,SWBPLCON0_6,SWBPLCON0_7,SWBPLCON0_8
	XDATA	SWBPLCON0_9,SWBPLCON0_10,SWBPLCON0_11,SWBPLCON0_12
	XDATA	SBMBPLCON0_1,SBMBPLCON0_2,SBMBPLCON0_1b,SBMBPLCON0_2b
	XDATA	TFBPLCON0_1,TFBPLCON0_2,TFBPLCON0_3,TFBPLCON0_4
	XDATA	TFBPLCON0_5,TFBPLCON0_6,TFBPLCON0_7,TFBPLCON0_8

;;	XDATA	AVEPlane

************************************************************************
	SECTION	,DATA_C		;HiSoft puts this Data into CHIP !!!!
	
	IFND	AACHIPS	

AVECopList1:
AVEintreq1	EQU	*+2-AVECopList1
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start

	CPMOVEDC	0,spr0pth	;fill in later
	CPMOVEDC	0,spr0ptl	;fill in later
	CPMOVEDC	0,spr1pth	;fill in later
	CPMOVEDC	0,spr1ptl	;fill in later

	CPMOVEDC	0,beamcon0	  ;make sure beamcon0 is ok, necessary?
	CPMOVEDC	NEWDENISE,bplcon3 ;fix new Denise right edge problem

AVE_LATCH	EQU	*-AVECopList1
AVECL1 equ *+2
	CPMOVEDC	0,cop1lch	;->AVE copper list body, fill in later
        CPMOVEDC	0,cop1lcl	;->AVE copper list body, fill in later

AVE_COLOR	EQU	*+2-AVECopList1
        CPMOVEDC   BLANKGRAY,color00		;filled in latter also
        CPMOVEDC   BLANKGRAY,color01	;necessary ???????

* Low contrast sprite. 
;;        CPMOVEDC   $776,color16   I=0 B=0 G=0 R=0 ;transparent
        CPMOVEDC   $876,color17   I=0 B=0 G=0 R=1
        CPMOVEDC   $786,color18   I=0 B=0 G=1 R=0
        CPMOVEDC   $886,color19   I=0 B=0 G=1 R=1
        CPMOVEDC   $778,color20   I=0 B=1 G=0 R=0
        CPMOVEDC   $878,color21   I=0 B=1 G=0 R=1
        CPMOVEDC   $788,color22   I=0 B=1 G=1 R=0
        CPMOVEDC   $888,color23   I=0 B=1 G=1 R=1
        CPMOVEDC   $777,color24   I=1 B=0 G=0 R=0
        CPMOVEDC   $877,color25   I=1 B=0 G=0 R=1
        CPMOVEDC   $787,color26   I=1 B=0 G=1 R=0
        CPMOVEDC   $887,color27   I=1 B=0 G=1 R=1
        CPMOVEDC   $779,color28   I=1 B=1 G=0 R=0
        CPMOVEDC   $879,color29   I=1 B=1 G=0 R=1
        CPMOVEDC   $789,color30   I=1 B=1 G=1 R=0
        CPMOVEDC   $889,color31   I=1 B=1 G=1 R=1

        CPMOVEDC   ((21<<8)!92),diwstrt
        CPMOVEDC   (((262<<8)&$ff00)!(458-256)),diwstop
        CPMOVEDC   ((64>>1)&$fc),ddfstrt
        CPMOVEDC   ((72>>1)&$fc),ddfstop
        CPMOVEDC   ((3<<4)!3),bplcon1   ;scroll
        CPMOVEDC   -8,bpl1mod		
        CPMOVEDC   $24,bplcon2		;sprite priority over bm

       	CPMOVEDC	0,copjmp1	;jump to copper list body

AVECopListMain1:
AVEintreq2	EQU	*+2-AVECopList1
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start

AVESPR 	EQU *-AVECopList1
	CPMOVEDC	0,spr0pth	;fill in later
	CPMOVEDC	0,spr0ptl	;fill in later
	CPMOVEDC	0,spr1pth	;fill in later
	CPMOVEDC	0,spr1ptl	;fill in later

AVE_BODY EQU *-AVECopList1
AVECopListBody1:
	CPMOVEDC	$0,cop1lch	;->AVE copper list body, fill in later
        CPMOVEDC	$0,cop1lcl	;->AVE copper list body, fill in later

	CPMOVEDC	$0,bpl1pth	;fill in later
	CPMOVEDC	$0,bpl1ptl	;fill in later

	CPMOVEDC	0,spr2pth       ;fill in later
	CPMOVEDC   	0,spr2ptl       ;fill in later
	CPMOVEDC	0,spr3pth       ;fill in later
	CPMOVEDC	0,spr3ptl       ;fill in later
	CPMOVEDC	0,spr4pth       ;fill in later
	CPMOVEDC	0,spr4ptl       ;fill in later
	CPMOVEDC	0,spr5pth       ;fill in later
	CPMOVEDC	0,spr5ptl       ;fill in later
	CPMOVEDC	0,spr6pth       ;fill in later
	CPMOVEDC	0,spr6ptl       ;fill in later
	CPMOVEDC	0,spr7pth       ;fill in later
	CPMOVEDC	0,spr7ptl       ;fill in later
	
        CPWAITDC	21,0	;necessary ?????????

* Should this bplcon0 stuff be here or in the header ????
AVEBPLCON0_1 	EQU *+2
	CPMOVEDC	BPLCON0_DEPTH1,bplcon0	;modes & #planes

	CPENDDC

************************************************************************
AVECopList2:
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start

	CPMOVEDC	0,spr0pth	;fill in later
	CPMOVEDC	0,spr0ptl	    ;fill in later
	CPMOVEDC	0,spr1pth	;fill in later
	CPMOVEDC	0,spr1ptl	;fill in later

	CPMOVEDC	0,beamcon0	  ;make sure beamcon0 is ok, necessary?
	CPMOVEDC	NEWDENISE,bplcon3 ;fix new Denise right edge problem

AVECL2 equ *+2
	CPMOVEDC	0,cop1lch	;->AVE copper list body, fill in later
        CPMOVEDC	0,cop1lcl	;->AVE copper list body, fill in later

        CPMOVEDC   BLANKGRAY,color00
        CPMOVEDC   BLANKGRAY,color01

* Low contrast sprite. 
;;        CPMOVEDC   $776,color16   I=0 B=0 G=0 R=0 ;transparent
        CPMOVEDC   $876,color17   I=0 B=0 G=0 R=1
        CPMOVEDC   $786,color18   I=0 B=0 G=1 R=0
        CPMOVEDC   $886,color19   I=0 B=0 G=1 R=1
        CPMOVEDC   $778,color20   I=0 B=1 G=0 R=0
        CPMOVEDC   $878,color21   I=0 B=1 G=0 R=1
        CPMOVEDC   $788,color22   I=0 B=1 G=1 R=0
        CPMOVEDC   $888,color23   I=0 B=1 G=1 R=1
        CPMOVEDC   $777,color24   I=1 B=0 G=0 R=0
        CPMOVEDC   $877,color25   I=1 B=0 G=0 R=1
        CPMOVEDC   $787,color26   I=1 B=0 G=1 R=0
        CPMOVEDC   $887,color27   I=1 B=0 G=1 R=1
        CPMOVEDC   $779,color28   I=1 B=1 G=0 R=0
        CPMOVEDC   $879,color29   I=1 B=1 G=0 R=1
        CPMOVEDC   $789,color30   I=1 B=1 G=1 R=0
        CPMOVEDC   $889,color31   I=1 B=1 G=1 R=1

        CPMOVEDC   ((21<<8)!92),diwstrt
        CPMOVEDC   (((262<<8)&$ff00)!(458-256)),diwstop
        CPMOVEDC   ((64>>1)&$fc),ddfstrt
        CPMOVEDC   ((72>>1)&$fc),ddfstop
        CPMOVEDC   ((3<<4)!3),bplcon1   ;scroll
        CPMOVEDC   -8,bpl1mod		
        CPMOVEDC   $24,bplcon2		;sprite priority over bm

       	CPMOVEDC	0,copjmp1	;jump to copper list body

AVECopListMain2:
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start

	CPMOVEDC	0,spr0pth	;fill in later
	CPMOVEDC	0,spr0ptl	;fill in later
	CPMOVEDC	0,spr1pth	;fill in later
	CPMOVEDC	0,spr1ptl	;fill in later

AVECopListBody2:
	CPMOVEDC	$0,cop1lch	;->AVE copper list body, fill in later
        CPMOVEDC	$0,cop1lcl	;->AVE copper list body, fill in later

	CPMOVEDC	$0,bpl1pth	;fill in later
	CPMOVEDC	$0,bpl1ptl	;fill in later

	CPMOVEDC	0,spr2pth       ;fill in later
	CPMOVEDC   	0,spr2ptl       ;fill in later
	CPMOVEDC	0,spr3pth       ;fill in later
	CPMOVEDC	0,spr3ptl       ;fill in later
	CPMOVEDC	0,spr4pth       ;fill in later
	CPMOVEDC	0,spr4ptl       ;fill in later
	CPMOVEDC	0,spr5pth       ;fill in later
	CPMOVEDC	0,spr5ptl       ;fill in later
	CPMOVEDC	0,spr6pth       ;fill in later
	CPMOVEDC	0,spr6ptl       ;fill in later
	CPMOVEDC	0,spr7pth       ;fill in later
	CPMOVEDC	0,spr7ptl       ;fill in later
	
        CPWAITDC	21,0	;necessary ?????????

* Should this bplcon0 stuff be here or in the header ????
AVEBPLCON0_2 	EQU *+2
	CPMOVEDC	BPLCON0_DEPTH1,bplcon0	;modes & #planes

	CPENDDC

****************************************************************
AVEInterface1:
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start
	CPMOVEDC	0,spr0pth	;fill in later
	CPMOVEDC	0,spr0ptl	;fill in later
	CPMOVEDC	0,spr1pth	;fill in later
	CPMOVEDC	0,spr1ptl	;fill in later

	CPMOVEDC	0,beamcon0	  ;make sure beamcon0 is ok, necessary?
	CPMOVEDC	NEWDENISE,bplcon3 ;fix new Denise right edge problem


AVEI_LATCH	EQU	*-AVEInterface1
AVEI1: equ *+2
	CPMOVEDC	$0,cop1lch	;->AVE copper list body, fill in later
        CPMOVEDC	$0,cop1lcl	;->AVE copper list body, fill in later

*----------------
AVEI_PALETTE	EQU	*+2-AVEInterface1
           CPMOVEDC   $000,color00	;fill these colors in later
           CPMOVEDC   $842,color01
           CPMOVEDC   $7bc,color02
           CPMOVEDC   $fff,color03
           CPMOVEDC   $000,color04
           CPMOVEDC   $000,color05
           CPMOVEDC   $000,color06
           CPMOVEDC   $000,color07
           CPMOVEDC   $000,color08
           CPMOVEDC   $000,color09
           CPMOVEDC   $000,color10
           CPMOVEDC   $000,color11
           CPMOVEDC   $000,color12
           CPMOVEDC   $000,color13
           CPMOVEDC   $000,color14
           CPMOVEDC   $000,color15

* Low contrast sprite. 
*;;          CPMOVEDC   $776,color16   I=0 B=0 G=0 R=0 ;transparent
          CPMOVEDC   $876,color17   ;I=0 B=0 G=0 R=1
          CPMOVEDC   $786,color18   ;I=0 B=0 G=1 R=0
          CPMOVEDC   $886,color19   ;I=0 B=0 G=1 R=1
          CPMOVEDC   $778,color20   ;I=0 B=1 G=0 R=0
          CPMOVEDC   $878,color21   ;I=0 B=1 G=0 R=1
          CPMOVEDC   $788,color22   ;I=0 B=1 G=1 R=0
          CPMOVEDC   $888,color23   ;I=0 B=1 G=1 R=1
          CPMOVEDC   $777,color24   ;I=1 B=0 G=0 R=0
          CPMOVEDC   $877,color25   ;I=1 B=0 G=0 R=1
          CPMOVEDC   $787,color26   ;I=1 B=0 G=1 R=0
          CPMOVEDC   $887,color27   ;I=1 B=0 G=1 R=1
          CPMOVEDC   $779,color28   ;I=1 B=1 G=0 R=0
          CPMOVEDC   $879,color29   ;I=1 B=1 G=0 R=1
          CPMOVEDC   $789,color30   ;I=1 B=1 G=1 R=0
          CPMOVEDC   $889,color31   ;I=1 B=1 G=1 R=1

        CPMOVEDC   ((21<<8)!92),diwstrt
        CPMOVEDC   (((262<<8)&$ff00)!(458-256)),diwstop
          CPMOVEDC   ((64>>1)&$fc),ddfstrt
          CPMOVEDC   ((424>>1)&$fc),ddfstop
          CPMOVEDC   ((7<<4)!7),bplcon1   ;scroll
          CPMOVEDC   96,bpl1mod
          CPMOVEDC   96,bpl2mod
          CPMOVEDC   $24,bplcon2         ;sprite priority over bm

       	CPMOVEDC	0,copjmp1	;jump to copper list body
*;-------------
AVEInterfaceMainI1:
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start

AVEISPR1 	EQU *-AVEInterface1
	CPMOVEDC	0,spr0pth	;fill in later
	CPMOVEDC	0,spr0ptl	;fill in later
	CPMOVEDC	0,spr1pth	;fill in later
	CPMOVEDC	0,spr1ptl	;fill in later

AVEI_IBODY	EQU	*-AVEInterface1
AVEInterfaceBody1:
	CPMOVEDC	$0,cop1lch	;->AVE copper list body, fill in later
        CPMOVEDC	$0,cop1lcl	;->AVE copper list body, fill in later
	
	CPMOVEDC	$0,bpl1pth	;fill in later
	CPMOVEDC	$0,bpl1ptl	;fill in later
	CPMOVEDC	$0,bpl2pth	;fill in later
	CPMOVEDC	$0,bpl2ptl	;fill in later
	CPMOVEDC	$0,bpl3pth	;fill in later
	CPMOVEDC	$0,bpl3ptl	;fill in later
	CPMOVEDC	$0,bpl4pth	;fill in later
	CPMOVEDC	$0,bpl4ptl	;fill in later
			
	CPMOVEDC	0,spr2pth       ;fill in later
	CPMOVEDC   	0,spr2ptl       ;fill in later
	CPMOVEDC	0,spr3pth       ;fill in later
	CPMOVEDC	0,spr3ptl       ;fill in later
	CPMOVEDC	0,spr4pth       ;fill in later
	CPMOVEDC	0,spr4ptl       ;fill in later
	CPMOVEDC	0,spr5pth       ;fill in later
	CPMOVEDC	0,spr5ptl       ;fill in later
	CPMOVEDC	0,spr6pth       ;fill in later
	CPMOVEDC	0,spr6ptl       ;fill in later
	CPMOVEDC	0,spr7pth       ;fill in later
	CPMOVEDC	0,spr7ptl       ;fill in later
	
 ;;       CPWAITDC	21,0	;necessary ?????????

AVEI_IBPLCON0_1		EQU	*+2-AVEInterfaceMainI1
AVEI_IBPLCON0_1b	EQU	*+2-AVEInterface1
	CPMOVEDC	BPLCON0_DEPTH2,bplcon0	;modes & #planes

AVEI_I	EQU	*-AVEInterface1

	CPWAITDC	0,0	;forever, 0, or 255
	CPWAITDC	0,0	;wait to top of safe area

	CPMOVEDC	BPLCON0_DEPTH2,bplcon0	;modes & #planes=2-4

	CPWAITDC	0,0	;0, or 255
	CPWAITDC	0,0	;wait to past bottom of safe area

AVEI_IBPLCON0_2		EQU	*+2-AVEInterfaceMainI1
AVEI_IBPLCON0_2b	EQU	*+2-AVEInterface1
	CPMOVEDC	BPLCON0_DEPTH2,bplcon0	;modes & #planes

	CPENDDC

*;-------------
AVEInterfaceMainII1:
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start
AVEISPR2 	EQU *-AVEInterface1
	CPMOVEDC	0,spr0pth	;fill in later
	CPMOVEDC	0,spr0ptl	;fill in later
	CPMOVEDC	0,spr1pth	;fill in later
	CPMOVEDC	0,spr1ptl	;fill in later

