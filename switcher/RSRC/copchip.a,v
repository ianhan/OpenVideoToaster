	CPWAITDC	0,0	;wait to after 255?,    fill in later
	CPWAITDC	0,0	;wait to top of effect, fill in later

	CPMOVEDC	BPLCON0_DEPTH0,bplcon0	;modes & #planes

          CPENDDC

***********************************************************************
*
* 1plane 	Narrow		;line 0 of display
*		Narrow	ZT lines		
*		Narrow	etc.
*		Narrow  0planes
*
* 1-4planes	INTERFACE I lines	0planes near end of last line of I
*
* 1plane 	Narrow	
*		Narrow	ZB lines		
*		Narrow	etc.
*		Narrow  	;line 240 of display
*
***********************************************************************

ZT	set 22
I	set 200
ZB	set 19

TwoFieldInterface1:
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start
TFOS1:	EQU	*+2-TwoFieldInterface1
	CPMOVEDC	0,spr0pth	;fill in later
	CPMOVEDC	0,spr0ptl	;fill in later
	CPMOVEDC	0,spr1pth	;fill in later
	CPMOVEDC	0,spr1ptl	;fill in later

TwoFI_LATCH	EQU	*-TwoFieldInterface1
TFOS2:	EQU	*+2-TwoFieldInterface1
	CPMOVEDC	0,cop1lch	;->1st field to install, I or II
        CPMOVEDC	0,cop1lcl	;filled in by InstallFieldIorII

	CPMOVEDC   0,beamcon0	  	;make sure beamcon0 is ok, necessary?
	CPMOVEDC   NEWDENISE,bplcon3 	;fix new Denise right edge problem
        CPMOVEDC   ((21<<8)!92),diwstrt
        CPMOVEDC   (((262<<8)&$ff00)!(458-256)),diwstop
        CPMOVEDC   ((64>>1)&$fc),ddfstrt
        CPMOVEDC   $24,bplcon2		;sprite priority over bm


TFOS3:	EQU	*+2-TwoFieldInterface1
	CPMOVEDC	$0,color01	; $842
	CPMOVEDC	$0,color02	; $7bc
	CPMOVEDC	$0,color03	; $fff
	CPMOVEDC	$0,color04	 
	CPMOVEDC	$0,color05	
	CPMOVEDC	$0,color06	 
	CPMOVEDC	$0,color07	 
	CPMOVEDC	$0,color08	
	CPMOVEDC	$0,color09	 
	CPMOVEDC	$0,color10	 
	CPMOVEDC	$0,color11	
	CPMOVEDC	$0,color12	 
	CPMOVEDC	$0,color13	 
	CPMOVEDC	$0,color14	
	CPMOVEDC	$0,color15	 

* Low contrast sprite. 
;;        CPMOVEDC   $776,color16   I=0 B=0 G=0 R=0 ;transparent
        CPMOVEDC   $876,color17   I=0 B=0 G=0 R=1
        CPMOVEDC   $786,color18   I=0 B=0 G=1 R=0
        CPMOVEDC   $886,color19   I=0 B=0 G=1 R=1
        CPMOVEDC   $778,color20   I=0 B=1 G=0 R=0
        CPMOVEDC   $878,color21   I=0 B=1 G=0 R=1
        CPMOVEDC   $788,color22   I=0 B=1 G=1 R=0
        CPMOVEDC   $888,color23   I=0 B=1 G=1 R=1
        CPMOVEDC   $777,color24   I=1 B=0 G=0 R=0
        CPMOVEDC   $877,color25   I=1 B=0 G=0 R=1
        CPMOVEDC   $787,color26   I=1 B=0 G=1 R=0
        CPMOVEDC   $887,color27   I=1 B=0 G=1 R=1
        CPMOVEDC   $779,color28   I=1 B=1 G=0 R=0
        CPMOVEDC   $879,color29   I=1 B=1 G=0 R=1
        CPMOVEDC   $789,color30   I=1 B=1 G=1 R=0
        CPMOVEDC   $889,color31   I=1 B=1 G=1 R=1

       	CPMOVEDC	0,copjmp1	;jump to copper list body

* MAINI1 -------------------------
TFOS1I:	EQU	*-TwoFieldInterface1
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start

TwoFI_ISPR	EQU	*-TwoFieldInterface1
TFOS2I:	EQU	*+2-TwoFieldInterface1
	CPMOVEDC	0,spr0pth	;fill in later
	CPMOVEDC	0,spr0ptl	;fill in later
	CPMOVEDC	0,spr1pth	;fill in later
	CPMOVEDC	0,spr1ptl	;fill in later

TwoFI_IBODY	EQU	*-TwoFieldInterface1
TFOS3I:	EQU	*+2-TwoFieldInterface1
	CPMOVEDC	$0,cop1lch	;->next field, fill in later 
        CPMOVEDC	$0,cop1lcl	;

        CPMOVEDC   ((72>>1)&$fc),ddfstop
        CPMOVEDC   ((3<<4)!3),bplcon1   ;scroll
        CPMOVEDC   -8,bpl1mod		

TFOS4I:	EQU	*+2-TwoFieldInterface1
          CPMOVEDC   $0000,spr2pth       ;fill in later
          CPMOVEDC   $0000,spr2ptl       ;fill in later
          CPMOVEDC   $0000,spr3pth       ;fill in later
          CPMOVEDC   $0000,spr3ptl       ;fill in later
          CPMOVEDC   $0000,spr4pth       ;fill in later
          CPMOVEDC   $0000,spr4ptl       ;fill in later
          CPMOVEDC   $0000,spr5pth       ;fill in later
          CPMOVEDC   $0000,spr5ptl       ;fill in later
          CPMOVEDC   $0000,spr6pth       ;fill in later
          CPMOVEDC   $0000,spr6ptl       ;fill in later
          CPMOVEDC   $0000,spr7pth       ;fill in later
          CPMOVEDC   $0000,spr7ptl       ;fill in later

TFOS5I:	EQU	*+2-TwoFieldInterface1
	CPMOVEDC	$0,color00	;$842 dark gray (not black), border

TFOS6I:	EQU	*+2-TwoFieldInterface1
 	CPMOVEDC	$0000,bpl1pth       ;fill in later
	CPMOVEDC	$0000,bpl1ptl       ;fill in later

TFBPLCON0_1:	EQU	*+2-TwoFieldInterface1
	CPMOVEDC	BPLCON0_DEPTH1,bplcon0	;modes & #planes

TFOS7I:	EQU	*-TwoFieldInterface1
	CPWAITDC	0,0		;wait for line 256??
        CPWAITDC	(21+ZT-1),19	;wait to after sprite, fill in later
	
TFBPLCON0_2:	EQU	*+2-TwoFieldInterface1
	CPMOVEDC	BPLCON0_DEPTH0,bplcon0	;modes & #planes

* from here until the next WAIT, we have time for approx.
* 44 moves, but we are doing must less, so we are OK

	CPMOVEDC	((424>>1)&$fc),ddfstop
	CPMOVEDC	((7<<4)!7),bplcon1		;scroll

TFOS8I:	EQU	*+2-TwoFieldInterface1
	CPMOVEDC	0,bpl1mod	;92 for 736 wide, 96 for 768 wide, fill in later
	CPMOVEDC	0,bpl2mod
	
TFOS9I:	EQU	*+2-TwoFieldInterface1
 	CPMOVEDC	$0000,bpl1pth       ;fill in later before install
	CPMOVEDC	$0000,bpl1ptl       ;fill in later
	CPMOVEDC	$0000,bpl2pth       ;fill in later
	CPMOVEDC	$0000,bpl2ptl       ;fill in later
	CPMOVEDC	$0000,bpl3pth       ;fill in later
	CPMOVEDC	$0000,bpl3ptl       ;fill in later
	CPMOVEDC	$0000,bpl4pth       ;fill in later
	CPMOVEDC	$0000,bpl4ptl       ;fill in later

* copper should be around horiz position 80
* wait to end of last line of ZT
TFOS10I: EQU	*-TwoFieldInterface1 
        CPWAITDC	(21+ZT-1),114	;fill in later

TFOS11I: EQU	*+2-TwoFieldInterface1
	CPMOVEDC	$000,color00	;fill this color in later

TFOS12I:	EQU	*-TwoFieldInterface1
	CPWAITDC	0,0		;wait for line 256??
        CPWAITDC	(21+ZT),0	;fill in later
	
TFOS13I:	EQU	*+2-TwoFieldInterface1

	CPMOVEDC	BPLCON0_DEPTH2,bplcon0	;modes & #planes=1-4

* Show interface
TFOS14I: EQU	*-TwoFieldInterface1
	CPWAITDC   0,0				;wait for line 255,15/111 ?
        CPWAITDC   (21+ZT+I-1),0		;wait to last line of interface
	CPMOVEDC   ((408>>1)&$fc),ddfstop  	;need to help abort 4 planes of dma

TFOS15I:	EQU	*-TwoFieldInterface1
        CPWAITDC	(21+ZT+I-1),109		;wait to near end of line, fill in later
						;can't be >109 or else sprite DMA will mess up
						;need time for 7 moves + wait
TFBPLCON0_3:	EQU	*+2-TwoFieldInterface1
	CPMOVEDC	BPLCON0_DEPTH0,bplcon0	;modes & #planes

TFOS16I:	EQU	*+2-TwoFieldInterface1
	CPMOVEDC	$0,color00	; $842 dark gray (not black), border

        CPMOVEDC   ((72>>1)&$fc),ddfstop
        CPMOVEDC   ((3<<4)!3),bplcon1   ;scroll
        CPMOVEDC   -8,bpl1mod		

TFOS17I:	EQU	*+2-TwoFieldInterface1
 	CPMOVEDC	$0000,bpl1pth       ;fill in later
	CPMOVEDC	$0000,bpl1ptl       ;fill in later

TFOS18I:	EQU	*-TwoFieldInterface1
	CPWAITDC   0,0			    ;wait for line 256??
	CPWAITDC   (21+ZT+I),0		    ;fill in later

TFBPLCON0_4:	EQU	*+2-TwoFieldInterface1
	CPMOVEDC	BPLCON0_DEPTH1,bplcon0	;modes & #planes

	CPENDDC			;wait to bottom of screen

* MAINII1 -------------------------
TFOS1II:	EQU	*-TwoFieldInterface1
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start

TwoFI_IISPR	EQU	*-TwoFieldInterface1
