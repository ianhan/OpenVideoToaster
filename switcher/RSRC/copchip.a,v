        CPMOVEDC   ((72>>1)&$fc),ddfstop
        CPMOVEDC   ((3<<4)!3),bplcon1   ;scroll
        CPMOVEDC   -8,bpl1mod		

 	CPMOVEDC	$0000,bpl1pth       ;fill in later
	CPMOVEDC	$0000,bpl1ptl       ;fill in later

	CPWAITDC   (21+4+DT+4+ZT+I+ZB+4+DB+1-256),0	;fill in later

	CPMOVEDC	BPLCON0_DEPTH1,bplcon0	;modes & #planes

* do headers HDR3, HDR1, HDR2

	CPENDDC

*********************************************************
* This could be improved to animate the BM in 2-D, and to support
* 4 bit planes.
* This CopList is used by ReadScanLine.
SimpleBMCopList1:
SSBMintreq1	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start

SBM1	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	0,spr0pth	;fill in later #
	CPMOVEDC	0,spr0ptl	;fill in later #
	CPMOVEDC	0,spr1pth	;fill in later #
	CPMOVEDC	0,spr1ptl	;fill in later #

	CPMOVEDC	0,beamcon0	  ;make sure beamcon0 is ok, necessary?
	CPMOVEDC        NEWDENISE,bplcon3 ;fix new Denise right edge problem

SBM_LATCH	EQU	*-SimpleBMCopList1
SBM2	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	$0,cop1lch	;->AVE copper list body, fill in later #
        CPMOVEDC	$0,cop1lcl	;->AVE copper list body, fill in later #

*---------
SBM7	EQU	*+2-SimpleBMCopList1
          CPMOVEDC   $000,color00       ;fill in later #
          CPMOVEDC   $000,color01       ;fill in later #
          CPMOVEDC   $000,color02       ;fill in later #
          CPMOVEDC   $000,color03       ;fill in later #
          CPMOVEDC   $000,color04       ;fill in later #
          CPMOVEDC   $000,color05       ;fill in later #
          CPMOVEDC   $000,color06       ;fill in later #
          CPMOVEDC   $000,color07       ;fill in later #
          CPMOVEDC   $000,color08       ;fill in later #
          CPMOVEDC   $000,color09       ;fill in later #
          CPMOVEDC   $000,color10       ;fill in later #
          CPMOVEDC   $000,color11       ;fill in later #
          CPMOVEDC   $000,color12       ;fill in later #
          CPMOVEDC   $000,color13       ;fill in later #
          CPMOVEDC   $000,color14       ;fill in later #
          CPMOVEDC   $000,color15       ;fill in later #

* Low contrast sprite (or bitmap palette if 32 color mode)
* We may not support 32 color mode because it will mess up the sprite!!!!
SBM3	EQU	*+2-SimpleBMCopList1
          CPMOVEDC   $776,color16   I=0 B=0 G=0 R=0 ;transparent
          CPMOVEDC   $876,color17   ;I=0 B=0 G=0 R=1
          CPMOVEDC   $786,color18   ;I=0 B=0 G=1 R=0
          CPMOVEDC   $886,color19   ;I=0 B=0 G=1 R=1
          CPMOVEDC   $778,color20   ;I=0 B=1 G=0 R=0
          CPMOVEDC   $878,color21   ;I=0 B=1 G=0 R=1
          CPMOVEDC   $788,color22   ;I=0 B=1 G=1 R=0
          CPMOVEDC   $888,color23   ;I=0 B=1 G=1 R=1
          CPMOVEDC   $777,color24   ;I=1 B=0 G=0 R=0
          CPMOVEDC   $877,color25   ;I=1 B=0 G=0 R=1
          CPMOVEDC   $787,color26   ;I=1 B=0 G=1 R=0
          CPMOVEDC   $887,color27   ;I=1 B=0 G=1 R=1
          CPMOVEDC   $779,color28   ;I=1 B=1 G=0 R=0
          CPMOVEDC   $879,color29   ;I=1 B=1 G=0 R=1
          CPMOVEDC   $789,color30   ;I=1 B=1 G=1 R=0
          CPMOVEDC   $889,color31   ;I=1 B=1 G=1 R=1

SBM8	EQU	*+2-SimpleBMCopList1
          CPMOVEDC   ((0<<4)!0),bplcon1 ;scroll, 	  ;fill in later #
          CPMOVEDC   0,bpl1mod				  ;fill in later #
          CPMOVEDC   0,bpl2mod				  ;fill in later #
          CPMOVEDC   ((64>>1)&$fc),ddfstrt		  ;fill in later #
          CPMOVEDC   ((424>>1)&$fc),ddfstop		  ;fill in later #

        CPMOVEDC   ((21<<8)!92),diwstrt
        CPMOVEDC   (((262<<8)&$ff00)!(458-256)),diwstop

SBM8b	EQU	*+2-SimpleBMCopList1	;notused
          CPMOVEDC   $24,bplcon2         ;sprite priority over bm

       	CPMOVEDC	0,copjmp1	;jump to copper list body
*---------
SBMMain1:
SSBMintreq2	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq        ;copper interrupt = flag start

SBMSPR	EQU	*-SimpleBMCopList1
	CPMOVEDC	0,spr0pth	;fill in later #
	CPMOVEDC	0,spr0ptl	;fill in later #
	CPMOVEDC	0,spr1pth	;fill in later #
	CPMOVEDC	0,spr1ptl	;fill in later #

*---------
SBM_IBODY	EQU	*-SimpleBMCopList1
SBMBody1:
SBM4	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	$0,cop1lch	;->AVE copper list body, fill in later #
        CPMOVEDC	$0,cop1lcl	;->AVE copper list body, fill in later #

SBM5	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	$0,bpl1pth	;fill in later #
	CPMOVEDC	$0,bpl1ptl	;fill in later #
	CPMOVEDC	$0,bpl2pth	;fill in later #
	CPMOVEDC	$0,bpl2ptl	;fill in later #
	CPMOVEDC	$0,bpl3pth	;fill in later #
	CPMOVEDC	$0,bpl3ptl	;fill in later #
	CPMOVEDC	$0,bpl4pth	;fill in later #
	CPMOVEDC	$0,bpl4ptl	;fill in later #
	CPMOVEDC	$0,bpl5pth	;fill in later #
	CPMOVEDC	$0,bpl5ptl	;fill in later #
	CPMOVEDC	$0,bpl6pth	;fill in later #
	CPMOVEDC	$0,bpl6ptl	;fill in later #

SBM6	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	0,spr2pth       ;fill in later #
	CPMOVEDC   	0,spr2ptl       ;fill in later #
	CPMOVEDC	0,spr3pth       ;fill in later #
	CPMOVEDC	0,spr3ptl       ;fill in later #
	CPMOVEDC	0,spr4pth       ;fill in later #
	CPMOVEDC	0,spr4ptl       ;fill in later #
	CPMOVEDC	0,spr5pth       ;fill in later #
	CPMOVEDC	0,spr5ptl       ;fill in later #
	CPMOVEDC	0,spr6pth       ;fill in later #
	CPMOVEDC	0,spr6ptl       ;fill in later #
	CPMOVEDC	0,spr7pth       ;fill in later #
	CPMOVEDC	0,spr7ptl       ;fill in later #

SBMBPLCON0_1	EQU	*+2-SimpleBMCopList1

* On nonAA machines it would have been best if this DEPTH was one, with
* color01 forced to color00, and using the narrow null BM, like we do
* in the TwoFieldInterfaceCopLists.  This would be useful if we wanted to
* position a short BM vertically, and still be able to use full height
* Sprites.  We never needed to do this in VT2.0.
* For VT3.0 vertical positioning of short BMs, with full height sprites
* is possible, but only on AA machines.

	CPMOVEDC	BPLCON0_DEPTH0,bplcon0	;modes & #planes

SBM9	EQU	*-SimpleBMCopList1
	CPWAITDC	0,0	;wait to after 255?,    fill in later
	CPWAITDC	0,0	;wait to top of effect, fill in later

	CPMOVEDC	BPLCON0_DEPTH0,bplcon0	;modes & #planes=1-4, fill in later #

	CPWAITDC	0,0	;wait to after 255?,    fill in later
	CPWAITDC	0,0	;wait to top of effect, fill in later

SBMBPLCON0_2	EQU	*+2-SimpleBMCopList1
	CPMOVEDC	BPLCON0_DEPTH0,bplcon0	;modes & #planes

          CPENDDC
*---------
