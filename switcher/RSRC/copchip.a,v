	CPMOVEDC	BPLCON0_DEPTH4,bplcon0	;modes & #planes

* do Top write
SWOS6:	EQU	*-SafeWriteCopList1
	CPWAITDC	0,0		;wait for line 256?? #
	CPWAITDC   (21+4+DT),0	;fill in later #

	CPMOVEDC   ((80>>1)&$fc),ddfstop  ;wait to after sprite (sooner??)
					  ;must be between 72 >=< 87
* do HDR0
SWOS7:	EQU	*-SafeWriteCopList1
	CPWAITDC   (21+4+DT),19	;fill in later #	;must be >=17

SWBPLCON0_4	EQU	*+2-SafeWriteCopList1
	CPMOVEDC	BPLCON0_DEPTH0,bplcon0	;modes & #planes
	
