	CPMOVEDC	$0000,bpl7pth       ;fill in later
	CPMOVEDC	$0000,bpl7ptl       ;fill in later
	CPMOVEDC	$0000,bpl8pth       ;fill in later
	CPMOVEDC	$0000,bpl8ptl       ;fill in later

	AAbplcon3	0,1		;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

* copper should be around horiz position 80
* wait to end of last line of ZT
        CPWAITDC	(21+ZT-1),112	;fill in later

        CPMOVEDC	0,color00	;#

	AAbplcon3	0,0		;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3
        CPMOVEDC	0,color00	;#

	CPWAITDC	0,0		;wait for line 256??
        CPWAITDC	(21+ZT),0	;fill in later
	
	CPMOVEDC	0,bplcon0	;modes & #planes=1-8 Fill in later #*

* Show interface
	CPWAITDC   0,0				;wait for line 255,15/111 ?
        CPWAITDC   (21+ZT+I-1),0		;wait to last line of interface

;;	CPMOVEDC   ((408>>1)&$fc),ddfstop  	;need to help abort 4 planes of dma NOT NEEDED ON AA MACHINE

	AAbplcon3	0,1		;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

        CPWAITDC	(21+ZT+I-1),109		;wait to near end of line, fill in later
###---RUDE---### FUCK 
						;can't be >109 or else sprite DMA will mess up
						;need time for 7 moves + wait	AAbplcon0	0,1,0,0		;depth=0, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes					;need time for 7 moves + wait	

        CPMOVEDC	0,color00	;$842 dark gray (not black), border #*
	AAbplcon3	0,0		;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3
        CPMOVEDC	0,color00	;low contrast write #*

	AAddfstop	AA16fetchStop
        CPMOVEDC	AAvalue,ddfstop
	
	AAbplcon1	AAscroll,AAscroll
        CPMOVEDC	AAvalue,bplcon1   	;scroll (put bm under sprite)

        CPMOVEDC   	AA16modulo,bpl1mod

 	CPMOVEDC	$0000,bpl1pth       ;fill in later
	CPMOVEDC	$0000,bpl1ptl       ;fill in later

	CPWAITDC   0,0			    ;wait for line 256??
	CPWAITDC   (21+ZT+I),0		    ;fill in later

	AAbplcon0	1,1,0,0		;depth=1, hires, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes

	CPENDDC			;wait to bottom of screen

	ENDC	;IFD AACHIPS

*************************************************************
*************************************************************
*************************************************************
* I've decide to put EffectsChipData into a CHIP Section
* This stuff may be addressed via ESD = EffectsChipData structure
* A pointer here is found in TB_EffectsBase+EFB_SpriteData

SpriteData:

* NOTE: We have two Live, and two Frozen sprites, so that we can double
* buffer these.  We needed to change sprite VStart in a double buffered way.
* Other than VStart, (and end zero LONG??), these sprites never get altered.
Sprite0LiveA	dc.l	$152d0603
		dcb.l	SPRITEHEIGHT,$f000f000
		dc.l	0

Sprite0LiveB	dc.l	$152d0603
		dcb.l	SPRITEHEIGHT,$f000f000
		dc.l	0

Sprite0FreezeA	dc.l	$152d0603
		dcb.l	SPRITEHEIGHT,$f0007000
		dc.l	0

Sprite0FreezeB	dc.l	$152d0603
		dcb.l	SPRITEHEIGHT,$f0007000
		dc.l	0

* These DVE sprites are used by Digital effects which require line#s.
* These should only contain line#s and no ELH stuff, unless you temporarily
* use them, then restore them later.
;;DVESprite0A	dc.l	$152d0603
;;		dcb.l	SPRITEHEIGHT,0
;;		dc.l	0
;;DVESprite0B	dc.l	$152d0603
;;		dcb.l	SPRITEHEIGHT,0
;;		dc.l	0

* These are shared by AVEI LUTS, LUT WIPES, and DVE.
* InstallAVE, AVEI, etc will make "NONCURRENT" DVESprites = TB_CurrentSprite1
* NOTE!!! I think it would be best to leave TB_CurrentSprite1 unchanged
* when installing AVE, AVEI etc., though after Reading/Writing this does allow
* for TB_CurrentSprite1 to be restored to its previous DVESprite state.
*
* Are there problems with DVE destination crop????
DVESprite1A	dc.l	$152d0683
		dcb.l	SPRITEHEIGHT,0
		dc.l	0

DVESprite1B	dc.l	$152d0683
		dcb.l	SPRITEHEIGHT,0
		dc.l	0

*-------------------------
* You may use these sprites at anytime, but remember these could have garbage
* in them, so you need to remake the entire sprite.
* Used by ReadScanLine, ReadScatter, DoSafeWrite
VTSprite0A	dc.l	$152d0603
		dcb.l	SPRITEHEIGHT,0
		dc.l	0
VTSprite0B	dc.l	$152d0603
		dcb.l	SPRITEHEIGHT,0
		dc.l	0

VTSprite1A	dc.l	$152d0683
		dcb.l	SPRITEHEIGHT,0
		dc.l	0

VTSprite1B	dc.l	$152d0683
		dcb.l	SPRITEHEIGHT,0
		dc.l	0

* Only used by SendELH2Toaster type routines.  These are for the most
* part private to the ELH routines.  These are always available by the ELH
* routines, so that the DVESprites may be unaltered when sending ELH stuff.
ELHSprite0A	dc.l	$152d0603
		dcb.l	SPRITEHEIGHT,0
		dc.l	0

ELHSprite0B	dc.l	$152d0603
		dcb.l	SPRITEHEIGHT,0
		dc.l	0

ELHSprite1A	dc.l	$152d0683
		dcb.l	SPRITEHEIGHT,0
		dc.l	0

ELHSprite1B	dc.l	$152d0683
		dcb.l	SPRITEHEIGHT,0
		dc.l	0

* NOTE!!!  For some unknown reason, these cleared longs must go here inorder
* for the non-68K framegrabing to not grab some garbage.  The null sprite/bm
* used to go here, but I wanted to move them.
 	
	dcb.l	2,0

* This is not currently shared by anyone, so the Ctrl LONG is alway correct!!!
* Used by 68000 FrameRead , old Field Write
Sprite1Linear  dc.w  $152d,$0683
;$Bm 0 0 0 lsnib, $Im 0 0 0 msnib
 dc.w $0000,$0000,$1000,$0000,$2000,$0000,$3000,$0000
 dc.w $4000,$0000,$5000,$0000,$6000,$0000,$7000,$0000
 dc.w $8000,$0000,$9000,$0000,$a000,$0000,$b000,$0000
 dc.w $c000,$0000,$d000,$0000,$e000,$0000,$f000,$0000

 dc.w $0000,$1000,$1000,$1000,$2000,$1000,$3000,$1000
 dc.w $4000,$1000,$5000,$1000,$6000,$1000,$7000,$1000
 dc.w $8000,$1000,$9000,$1000,$a000,$1000,$b000,$1000
 dc.w $c000,$1000,$d000,$1000,$e000,$1000,$f000,$1000

 dc.w $0000,$2000,$1000,$2000,$2000,$2000,$3000,$2000
 dc.w $4000,$2000,$5000,$2000,$6000,$2000,$7000,$2000
 dc.w $8000,$2000,$9000,$2000,$a000,$2000,$b000,$2000
 dc.w $c000,$2000,$d000,$2000,$e000,$2000,$f000,$2000

 dc.w $0000,$3000,$1000,$3000,$2000,$3000,$3000,$3000
 dc.w $4000,$3000,$5000,$3000,$6000,$3000,$7000,$3000
 dc.w $8000,$3000,$9000,$3000,$a000,$3000,$b000,$3000
 dc.w $c000,$3000,$d000,$3000,$e000,$3000,$f000,$3000

 dc.w $0000,$4000,$1000,$4000,$2000,$4000,$3000,$4000
 dc.w $4000,$4000,$5000,$4000,$6000,$4000,$7000,$4000
 dc.w $8000,$4000,$9000,$4000,$a000,$4000,$b000,$4000
 dc.w $c000,$4000,$d000,$4000,$e000,$4000,$f000,$4000

 dc.w $0000,$5000,$1000,$5000,$2000,$5000,$3000,$5000
 dc.w $4000,$5000,$5000,$5000,$6000,$5000,$7000,$5000
 dc.w $8000,$5000,$9000,$5000,$a000,$5000,$b000,$5000
 dc.w $c000,$5000,$d000,$5000,$e000,$5000,$f000,$5000

 dc.w $0000,$6000,$1000,$6000,$2000,$6000,$3000,$6000
 dc.w $4000,$6000,$5000,$6000,$6000,$6000,$7000,$6000
 dc.w $8000,$6000,$9000,$6000,$a000,$6000,$b000,$6000
 dc.w $c000,$6000,$d000,$6000,$e000,$6000,$f000,$6000

 dc.w $0000,$7000,$1000,$7000,$2000,$7000,$3000,$7000
 dc.w $4000,$7000,$5000,$7000,$6000,$7000,$7000,$7000
 dc.w $8000,$7000,$9000,$7000,$a000,$7000,$b000,$7000
 dc.w $c000,$7000,$d000,$7000,$e000,$7000,$f000,$7000

 dc.w $0000,$8000,$1000,$8000,$2000,$8000,$3000,$8000
 dc.w $4000,$8000,$5000,$8000,$6000,$8000,$7000,$8000
 dc.w $8000,$8000,$9000,$8000,$a000,$8000,$b000,$8000
 dc.w $c000,$8000,$d000,$8000,$e000,$8000,$f000,$8000

 dc.w $0000,$9000,$1000,$9000,$2000,$9000,$3000,$9000
 dc.w $4000,$9000,$5000,$9000,$6000,$9000,$7000,$9000
 dc.w $8000,$9000,$9000,$9000,$a000,$9000,$b000,$9000
 dc.w $c000,$9000,$d000,$9000,$e000,$9000,$f000,$9000

 dc.w $0000,$a000,$1000,$a000,$2000,$a000,$3000,$a000
 dc.w $4000,$a000,$5000,$a000,$6000,$a000,$7000,$a000
 dc.w $8000,$a000,$9000,$a000,$a000,$a000,$b000,$a000
 dc.w $c000,$a000,$d000,$a000,$e000,$a000,$f000,$a000

 dc.w $0000,$b000,$1000,$b000,$2000,$b000,$3000,$b000
 dc.w $4000,$b000,$5000,$b000,$6000,$b000,$7000,$b000
 dc.w $8000,$b000,$9000,$b000,$a000,$b000,$b000,$b000
 dc.w $c000,$b000,$d000,$b000,$e000,$b000,$f000,$b000

 dc.w $0000,$c000,$1000,$c000,$2000,$c000,$3000,$c000
 dc.w $4000,$c000,$5000,$c000,$6000,$c000,$7000,$c000
 dc.w $8000,$c000,$9000,$c000,$a000,$c000,$b000,$c000
 dc.w $c000,$c000,$d000,$c000,$e000,$c000,$f000,$c000

 dc.w $0000,$d000,$1000,$d000,$2000,$d000,$3000,$d000
 dc.w $4000,$d000,$5000,$d000,$6000,$d000,$7000,$d000
 dc.w $8000,$d000,$9000,$d000,$a000,$d000,$b000,$d000
 dc.w $c000,$d000,$d000,$d000,$e000,$d000,$f000,$d000

 dc.w $0000,$e000,$1000,$e000,$2000,$e000,$3000,$e000
 dc.w $4000,$e000,$5000,$e000,$6000,$e000,$7000,$e000
 dc.w $8000,$e000,$9000,$e000,$a000,$e000,$b000,$e000
 dc.w $c000,$e000,$d000,$e000,$e000,$e000,$f000,$e000

 dc.w $0000,$f000
 dc.w  0,0

	IFND	AACHIPS
NarrowBMNull:
SpriteNull	dcb.l	2,0
	ENDC


	IFD	AACHIPS
	CNOP	0,8	;double long word align

SpriteNull	dc.l	0	;these longs also makes sure its clear
		dc.l	0 	;under the sprite.

* Double Long Word Aligned
NarrowBMNull:	;Actually on a A2000 the 4th byte will be right after sprite
		;On a A4000 the previous WORD will be under sprite
		
		dcb.l	6,0	;need extra wide null bm on A4000
	ENDC

@


