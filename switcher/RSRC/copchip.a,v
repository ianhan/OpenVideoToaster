SystemLink:
	CPMOVEDC	0,cop1lch	;->AVE copper list body, fill in later
        CPMOVEDC	0,cop1lcl	;->AVE copper list body, fill in later

	CPMOVEDC	BPLCON0_DEPTH0,bplcon0	;modes & #planes NO ERSY!!!!

       	CPMOVEDC	0,copjmp1	;jump to copper list body
	CPENDDC

********************************************************************
* Original 68000 FrameGrab coplist
* NOTE!!!!!! NOT USED ON AA MACHINES!!! I haven't killed this because
* InitReadCopList modifies this data.  So more than just this would need
* to be ripped out.

ReadCopList1:    CPMOVEDC   INTF_SETCLR!INTF_COPER,intreq        ;copper interrupt = flag start
;I=most sig nibble, B=least sig nibble,  NOTE: The data must be complemented

	CPMOVEDC	0,beamcon0	  ;make sure beamcon0 is ok, necessary?
    	  CPMOVEDC   NEWDENISE,bplcon3 ;fix new Denise right edge problem

          CPMOVEDC   $229,color00  ;I=1 B=1
          CPMOVEDC   $227,color01  ;I=1 B=0
          CPMOVEDC   $228,color02  ;I=0 B=1
          CPMOVEDC   $226,color03  ;I=0 B=0

* Low contrast sprite. 
;;          CPMOVEDC   $776,color16   I=0 B=0 G=0 R=0 ;transparent
          CPMOVEDC   $876,color17   I=0 B=0 G=0 R=1
          CPMOVEDC   $786,color18   I=0 B=0 G=1 R=0
          CPMOVEDC   $886,color19   I=0 B=0 G=1 R=1
          CPMOVEDC   $778,color20   I=0 B=1 G=0 R=0
          CPMOVEDC   $878,color21   I=0 B=1 G=0 R=1
          CPMOVEDC   $788,color22   I=0 B=1 G=1 R=0
          CPMOVEDC   $888,color23   I=0 B=1 G=1 R=1
          CPMOVEDC   $777,color24   I=1 B=0 G=0 R=0
          CPMOVEDC   $877,color25   I=1 B=0 G=0 R=1
          CPMOVEDC   $787,color26   I=1 B=0 G=1 R=0
          CPMOVEDC   $887,color27   I=1 B=0 G=1 R=1
          CPMOVEDC   $779,color28   I=1 B=1 G=0 R=0
          CPMOVEDC   $879,color29   I=1 B=1 G=0 R=1
          CPMOVEDC   $789,color30   I=1 B=1 G=1 R=0
          CPMOVEDC   $889,color31   I=1 B=1 G=1 R=1

        CPMOVEDC   ((21<<8)!92),diwstrt
        CPMOVEDC   (((262<<8)&$ff00)!(458-256)),diwstop
          CPMOVEDC   ((64>>1)&$fc),ddfstrt
          CPMOVEDC   ((424>>1)&$fc),ddfstop
          CPMOVEDC   ((7<<4)!7),bplcon1   ;scroll
          CPMOVEDC   -96,bpl1mod
          CPMOVEDC   -96,bpl2mod

          CPMOVEDC   $24,bplcon2         ;sprite priority over bm

ReadLCH1:  ;fill in CHeaderLCH+2, and CHeaderLCH+6
          CPMOVEDC   $0000,cop1lch       ;->main copper list, fill in later
          CPMOVEDC   $0000,cop1lcl       ;->main copper list, fill in later
          CPMOVEDC   0,copjmp1           ;jump to main copper list

;---------------------------------------------------------
; There is a copper interrupt PULSE at the start of this coplist.
ReadMain1: CPMOVEDC   INTF_SETCLR!INTF_COPER,intreq  ;redundant safety feature !!!
   
          CPMOVEDC   $0000,spr0pth       ;fill in later
          CPMOVEDC   $0000,spr0ptl       ;fill in later
          CPMOVEDC   $0000,spr1pth       ;fill in later
          CPMOVEDC   $0000,spr1ptl       ;fill in later
          CPMOVEDC   $0000,spr2pth       ;fill in later
          CPMOVEDC   $0000,spr2ptl       ;fill in later
          CPMOVEDC   $0000,spr3pth       ;fill in later
          CPMOVEDC   $0000,spr3ptl       ;fill in later
          CPMOVEDC   $0000,spr4pth       ;fill in later
          CPMOVEDC   $0000,spr4ptl       ;fill in later
          CPMOVEDC   $0000,spr5pth       ;fill in later
          CPMOVEDC   $0000,spr5ptl       ;fill in later
          CPMOVEDC   $0000,spr6pth       ;fill in later
          CPMOVEDC   $0000,spr6ptl       ;fill in later
          CPMOVEDC   $0000,spr7pth       ;fill in later
          CPMOVEDC   $0000,spr7ptl       ;fill in later

ReadBpl1:
          CPMOVEDC   $0000,bpl1pth       ;fill in later
          CPMOVEDC   $0000,bpl1ptl       ;fill in later
          CPMOVEDC   $0000,bpl2pth       ;fill in later
          CPMOVEDC   $0000,bpl2ptl       ;fill in later

	  CPWAITDC   21,0			

ReadBPLCON0_1	equ	*+2
	CPMOVEDC	BPLCON0_DEPTH2,bplcon0	;modes & #planes

          CPMOVEDC   INTF_COPER,intreq  ;clear copper interrupt = flag grab

          CPENDDC
;----------------------------------------------
ReadCopList2:    CPMOVEDC   INTF_SETCLR!INTF_COPER,intreq        ;copper interrupt = flag start
;I=most sig nibble, B=least sig nibble,  NOTE: The data must be complemented

	CPMOVEDC	0,beamcon0	  ;make sure beamcon0 is ok, necessary?
    	  CPMOVEDC   NEWDENISE,bplcon3 ;fix new Denise right edge problem

          CPMOVEDC   $229,color00  ;I=1 B=1
          CPMOVEDC   $227,color01  ;I=1 B=0
          CPMOVEDC   $228,color02  ;I=0 B=1
          CPMOVEDC   $226,color03  ;I=0 B=0

* Low contrast sprite. 
;;          CPMOVEDC   $776,color16   I=0 B=0 G=0 R=0 ;transparent
          CPMOVEDC   $876,color17   I=0 B=0 G=0 R=1
          CPMOVEDC   $786,color18   I=0 B=0 G=1 R=0
          CPMOVEDC   $886,color19   I=0 B=0 G=1 R=1
          CPMOVEDC   $778,color20   I=0 B=1 G=0 R=0
          CPMOVEDC   $878,color21   I=0 B=1 G=0 R=1
          CPMOVEDC   $788,color22   I=0 B=1 G=1 R=0
          CPMOVEDC   $888,color23   I=0 B=1 G=1 R=1
          CPMOVEDC   $777,color24   I=1 B=0 G=0 R=0
          CPMOVEDC   $877,color25   I=1 B=0 G=0 R=1
          CPMOVEDC   $787,color26   I=1 B=0 G=1 R=0
          CPMOVEDC   $887,color27   I=1 B=0 G=1 R=1
          CPMOVEDC   $779,color28   I=1 B=1 G=0 R=0
          CPMOVEDC   $879,color29   I=1 B=1 G=0 R=1
          CPMOVEDC   $789,color30   I=1 B=1 G=1 R=0
          CPMOVEDC   $889,color31   I=1 B=1 G=1 R=1

        CPMOVEDC   ((21<<8)!92),diwstrt
        CPMOVEDC   (((262<<8)&$ff00)!(458-256)),diwstop
          CPMOVEDC   ((64>>1)&$fc),ddfstrt
          CPMOVEDC   ((424>>1)&$fc),ddfstop
          CPMOVEDC   ((7<<4)!7),bplcon1   ;scroll
          CPMOVEDC   -96,bpl1mod
          CPMOVEDC   -96,bpl2mod

          CPMOVEDC   $24,bplcon2         ;sprite priority over bm

ReadLCH2:  ;fill in CHeaderLCH+2, and CHeaderLCH+6
          CPMOVEDC   $0000,cop1lch       ;->main copper list, fill in later
          CPMOVEDC   $0000,cop1lcl       ;->main copper list, fill in later
          CPMOVEDC   0,copjmp1           ;jump to main copper list

;---------------------------------------------------------

ReadMain2: CPMOVEDC   INTF_SETCLR!INTF_COPER,intreq  ;redundant safety feature !!!
   
          CPMOVEDC   $0000,spr0pth       ;fill in later
          CPMOVEDC   $0000,spr0ptl       ;fill in later
          CPMOVEDC   $0000,spr1pth       ;fill in later
          CPMOVEDC   $0000,spr1ptl       ;fill in later
          CPMOVEDC   $0000,spr2pth       ;fill in later
          CPMOVEDC   $0000,spr2ptl       ;fill in later
          CPMOVEDC   $0000,spr3pth       ;fill in later
          CPMOVEDC   $0000,spr3ptl       ;fill in later
          CPMOVEDC   $0000,spr4pth       ;fill in later
          CPMOVEDC   $0000,spr4ptl       ;fill in later
          CPMOVEDC   $0000,spr5pth       ;fill in later
          CPMOVEDC   $0000,spr5ptl       ;fill in later
          CPMOVEDC   $0000,spr6pth       ;fill in later
          CPMOVEDC   $0000,spr6ptl       ;fill in later
          CPMOVEDC   $0000,spr7pth       ;fill in later
          CPMOVEDC   $0000,spr7ptl       ;fill in later

ReadBpl2:
          CPMOVEDC   $0000,bpl1pth       ;fill in later
          CPMOVEDC   $0000,bpl1ptl       ;fill in later
          CPMOVEDC   $0000,bpl2pth       ;fill in later
          CPMOVEDC   $0000,bpl2ptl       ;fill in later

	  CPWAITDC   21,0						

ReadBPLCON0_2	equ	*+2
	CPMOVEDC	BPLCON0_DEPTH2,bplcon0	;modes & #planes

          CPMOVEDC   INTF_COPER,intreq  ;clear copper interrupt = flag grab

          CPENDDC

ReadCopperLists		dc.l	ReadCopList1,ReadCopList2	
ReadCopListPlanes	dc.l	ReadBpl1,ReadBpl2

*************************************************************************
;When Writing, I=Most sig bit of nibble, B, G, R=Least sig bit
FieldWriteCopList1:
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start
          CPMOVEDC   $0000,spr0pth       ;fill in later
          CPMOVEDC   $0000,spr0ptl       ;fill in later
          CPMOVEDC   $0000,spr1pth       ;fill in later
          CPMOVEDC   $0000,spr1ptl       ;fill in later

* window start & stops are put her to prevent premature starts
	AAdiwstrt	AAwindowStartX,AAwindowStartY					
        CPMOVEDC	AAvalue,diwstrt

	AAdiwstop	AAwindowStopX,AAwindowStopY
        CPMOVEDC	AAvalue,diwstop

	AAdiwhigh	AAwindowStartX,AAwindowStopX,AAwindowStartY,AAwindowStopY
	CPMOVEDC	AAvalue,diwhigh

FWPLANES  EQU	*+2-FieldWriteCopList1
          CPMOVEDC   $0000,bpl1pth       ;fill in later
          CPMOVEDC   $0000,bpl1ptl       ;fill in later
          CPMOVEDC   $0000,bpl2pth       ;fill in later
          CPMOVEDC   $0000,bpl2ptl       ;fill in later
          CPMOVEDC   $0000,bpl3pth       ;fill in later
          CPMOVEDC   $0000,bpl3ptl       ;fill in later
          CPMOVEDC   $0000,bpl4pth       ;fill in later
          CPMOVEDC   $0000,bpl4ptl       ;fill in later

	  CPMOVEDC   0,cop1lch	;->after Write copper list body, fill in later
          CPMOVEDC   0,cop1lcl	;->after Write copper list body, fill in later

FWMOD	EQU	*+2-FieldWriteCopList1
          CPMOVEDC   0,bpl1mod	;-96 for sync write, -4 for 736 wide, 0 for 768 wide, fill in later
          CPMOVEDC   0,bpl2mod

;Low contrast write
	AAbplcon3	0,1	;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

	CPMOVEDC   BLANKGRAY,color00	;color 0 is $842 dark gray (not black), border??? #*
        CPMOVEDC   BLANKGRAY,color01  
        CPMOVEDC   BLANKGRAY,color02  
        CPMOVEDC   BLANKGRAY,color03  
        CPMOVEDC   BLANKGRAY,color04  
        CPMOVEDC   BLANKGRAY,color05  
        CPMOVEDC   BLANKGRAY,color06  
        CPMOVEDC   BLANKGRAY,color07  
        CPMOVEDC   BLANKGRAY,color08  
        CPMOVEDC   BLANKGRAY,color09  
        CPMOVEDC   BLANKGRAY,color10  
        CPMOVEDC   BLANKGRAY,color11  
        CPMOVEDC   BLANKGRAY,color12  
        CPMOVEDC   BLANKGRAY,color13  
        CPMOVEDC   BLANKGRAY,color14  
        CPMOVEDC   BLANKGRAY,color15

	AAbplcon3	0,0	;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3

        CPMOVEDC   $000,color00   G0=0 R0=0 B2=0 R2=0	;color 0 is $842 dark gray (not black), border??? #*
        CPMOVEDC   $400,color01   G0=0 R0=0 B2=0 R2=1
        CPMOVEDC   $004,color02   G0=0 R0=0 B2=1 R2=0
        CPMOVEDC   $404,color03   G0=0 R0=0 B2=1 R2=1
        CPMOVEDC   $100,color04   G0=0 R0=1 B2=0 R2=0
        CPMOVEDC   $500,color05   G0=0 R0=1 B2=0 R2=1
        CPMOVEDC   $104,color06   G0=0 R0=1 B2=1 R2=0
        CPMOVEDC   $504,color07   G0=0 R0=1 B2=1 R2=1
        CPMOVEDC   $010,color08   G0=1 R0=0 B2=0 R2=0
        CPMOVEDC   $410,color09   G0=1 R0=0 B2=0 R2=1
        CPMOVEDC   $014,color10   G0=1 R0=0 B2=1 R2=0
        CPMOVEDC   $414,color11   G0=1 R0=0 B2=1 R2=1
        CPMOVEDC   $110,color12   G0=1 R0=1 B2=0 R2=0
        CPMOVEDC   $510,color13   G0=1 R0=1 B2=0 R2=1
        CPMOVEDC   $114,color14   G0=1 R0=1 B2=1 R2=0
        CPMOVEDC   $514,color15   G0=1 R0=1 B2=1 R2=1

	AAspritePalette

	AAddfstrt	AA768fetchStart
        CPMOVEDC	AAvalue,ddfstrt

	AAddfstop	AA768fetchStop
        CPMOVEDC	AAvalue,ddfstop

FWSCROLL	EQU	*+2-FieldWriteCopList1
        CPMOVEDC	0,bplcon1   		;scroll (put sprite left of bm) FILL IN LATER

        CPMOVEDC   	AAbplcon2,bplcon2	;sprite priority over bm
	CPMOVEDC	AAbplcon4,bplcon4	;sprite colors = 240-255
	CPMOVEDC	AA768fetchMode,fmode


FWBPLCON0_1	EQU	*+2
	AAbplcon0	4,1,0,0		;depth=1, Hires, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes

FWSPR1	EQU	*+2
          CPMOVEDC   $0000,spr2pth       ;fill in later
          CPMOVEDC   $0000,spr2ptl       ;fill in later
          CPMOVEDC   $0000,spr3pth       ;fill in later
          CPMOVEDC   $0000,spr3ptl       ;fill in later
          CPMOVEDC   $0000,spr4pth       ;fill in later
          CPMOVEDC   $0000,spr4ptl       ;fill in later
          CPMOVEDC   $0000,spr5pth       ;fill in later
          CPMOVEDC   $0000,spr5ptl       ;fill in later
          CPMOVEDC   $0000,spr6pth       ;fill in later
          CPMOVEDC   $0000,spr6ptl       ;fill in later
          CPMOVEDC   $0000,spr7pth       ;fill in later
          CPMOVEDC   $0000,spr7ptl       ;fill in later

          CPENDDC

;---------------------------------------------------------
;When Writing, I=Most sig bit of nibble, B, G, R=Least sig bit
FieldWriteCopList2:
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start
          CPMOVEDC   $0000,spr0pth       ;fill in later
          CPMOVEDC   $0000,spr0ptl       ;fill in later
          CPMOVEDC   $0000,spr1pth       ;fill in later
          CPMOVEDC   $0000,spr1ptl       ;fill in later

* window start & stops are put her to prevent premature starts
	AAdiwstrt	AAwindowStartX,AAwindowStartY					
        CPMOVEDC	AAvalue,diwstrt

	AAdiwstop	AAwindowStopX,AAwindowStopY
        CPMOVEDC	AAvalue,diwstop

	AAdiwhigh	AAwindowStartX,AAwindowStopX,AAwindowStartY,AAwindowStopY
	CPMOVEDC	AAvalue,diwhigh

          CPMOVEDC   $0000,bpl1pth       ;fill in later
          CPMOVEDC   $0000,bpl1ptl       ;fill in later
          CPMOVEDC   $0000,bpl2pth       ;fill in later
          CPMOVEDC   $0000,bpl2ptl       ;fill in later
          CPMOVEDC   $0000,bpl3pth       ;fill in later
          CPMOVEDC   $0000,bpl3ptl       ;fill in later
          CPMOVEDC   $0000,bpl4pth       ;fill in later
          CPMOVEDC   $0000,bpl4ptl       ;fill in later

	  CPMOVEDC   0,cop1lch	;->after Write copper list body, fill in later
          CPMOVEDC   0,cop1lcl	;->after Write copper list body, fill in later

          CPMOVEDC   0,bpl1mod	;-96 for sync write, -4 for 736 wide, 0 for 768 wide, fill in later
          CPMOVEDC   0,bpl2mod

;Low contrast write
	AAbplcon3	0,1	;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

	CPMOVEDC   BLANKGRAY,color00	;color 0 is $842 dark gray (not black), border??? #*
        CPMOVEDC   BLANKGRAY,color01  
        CPMOVEDC   BLANKGRAY,color02  
        CPMOVEDC   BLANKGRAY,color03  
        CPMOVEDC   BLANKGRAY,color04  
        CPMOVEDC   BLANKGRAY,color05  
        CPMOVEDC   BLANKGRAY,color06  
        CPMOVEDC   BLANKGRAY,color07  
        CPMOVEDC   BLANKGRAY,color08  
        CPMOVEDC   BLANKGRAY,color09  
        CPMOVEDC   BLANKGRAY,color10  
        CPMOVEDC   BLANKGRAY,color11  
        CPMOVEDC   BLANKGRAY,color12  
        CPMOVEDC   BLANKGRAY,color13  
        CPMOVEDC   BLANKGRAY,color14  
        CPMOVEDC   BLANKGRAY,color15

	AAbplcon3	0,0	;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3

        CPMOVEDC   $000,color00   G0=0 R0=0 B2=0 R2=0	;color 0 is $842 dark gray (not black), border??? #*
        CPMOVEDC   $400,color01   G0=0 R0=0 B2=0 R2=1
        CPMOVEDC   $004,color02   G0=0 R0=0 B2=1 R2=0
        CPMOVEDC   $404,color03   G0=0 R0=0 B2=1 R2=1
        CPMOVEDC   $100,color04   G0=0 R0=1 B2=0 R2=0
        CPMOVEDC   $500,color05   G0=0 R0=1 B2=0 R2=1
        CPMOVEDC   $104,color06   G0=0 R0=1 B2=1 R2=0
        CPMOVEDC   $504,color07   G0=0 R0=1 B2=1 R2=1
        CPMOVEDC   $010,color08   G0=1 R0=0 B2=0 R2=0
        CPMOVEDC   $410,color09   G0=1 R0=0 B2=0 R2=1
        CPMOVEDC   $014,color10   G0=1 R0=0 B2=1 R2=0
        CPMOVEDC   $414,color11   G0=1 R0=0 B2=1 R2=1
        CPMOVEDC   $110,color12   G0=1 R0=1 B2=0 R2=0
        CPMOVEDC   $510,color13   G0=1 R0=1 B2=0 R2=1
        CPMOVEDC   $114,color14   G0=1 R0=1 B2=1 R2=0
        CPMOVEDC   $514,color15   G0=1 R0=1 B2=1 R2=1

	AAspritePalette

	AAddfstrt	AA768fetchStart
        CPMOVEDC	AAvalue,ddfstrt

	AAddfstop	AA768fetchStop
        CPMOVEDC	AAvalue,ddfstop

        CPMOVEDC	0,bplcon1   		;scroll (put sprite left of bm) FILL IN LATER

        CPMOVEDC   	AAbplcon2,bplcon2	;sprite priority over bm
	CPMOVEDC	AAbplcon4,bplcon4	;sprite colors = 240-255
	CPMOVEDC	AA768fetchMode,fmode


FWBPLCON0_2	EQU	*+2
	AAbplcon0	4,1,0,0		;depth=1, Hires, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes

FWSPR2	EQU	*+2
          CPMOVEDC   $0000,spr2pth       ;fill in later
          CPMOVEDC   $0000,spr2ptl       ;fill in later
          CPMOVEDC   $0000,spr3pth       ;fill in later
          CPMOVEDC   $0000,spr3ptl       ;fill in later
          CPMOVEDC   $0000,spr4pth       ;fill in later
          CPMOVEDC   $0000,spr4ptl       ;fill in later
          CPMOVEDC   $0000,spr5pth       ;fill in later
          CPMOVEDC   $0000,spr5ptl       ;fill in later
          CPMOVEDC   $0000,spr6pth       ;fill in later
          CPMOVEDC   $0000,spr6ptl       ;fill in later
          CPMOVEDC   $0000,spr7pth       ;fill in later
          CPMOVEDC   $0000,spr7ptl       ;fill in later

          CPENDDC

***********************************************************************
*
* 1plane	HDR3	;line 0 of display	;matte to black
*		HDR1				;pv
*		HDR2				;am/bm
*		HDR0	0planes			
*
* 4planes	WRITE	DT lines >= 0
*		
*		HDR0	0planes
* 1plane	HDR2				;am/bm
*		HDR1				;pv
*		HDR3	0planes			;matte restore 
*
*		NULL 	ZT lines >= 0	
*
* optional end here
*
* 1-4planes	INTERFACE I lines	0planes near end of last line of I
*	
*		NULL	ZB lines >= 0
* 	
* optional end here
*
* 1plane	HDR3			;matte to black
*		HDR1			;pv
*		HDR2			;am/bm
*		HDR0	0planes		
*
* 4planes	WRITE	DB lines >= 0	
*
*		HDR0	0planes		
* 1plane	HDR2			;am/bm
*		HDR1			;pv
*		HDR3			;matte restore 
*		----	;line 240 of display
*
***********************************************************************


DT	set 2
ZT	set 0
I	set 200
ZB	set 0
DB	set 4

*----------------------------------------------------
SafeWriteCopList1:
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start
SWOS1:	EQU	*+2-SafeWriteCopList1
	CPMOVEDC	0,spr0pth	;fill in later #*
	CPMOVEDC	0,spr0ptl	;fill in later #*
	CPMOVEDC	0,spr1pth	;fill in later #*
	CPMOVEDC	0,spr1ptl	;fill in later #*

SWOS2:	EQU	*+2-SafeWriteCopList1
	CPMOVEDC	0,cop1lch	;->after Write copper list body, fill in later #*
        CPMOVEDC	0,cop1lcl	;->after Wrtte copper list body, fill in later #*

	AAdiwstrt	AAwindowStartX,AAwindowStartY					
        CPMOVEDC	AAvalue,diwstrt

SWOS2b:	EQU	*+2-SafeWriteCopList1
        CPMOVEDC	0,diwstop		;#*
	CPMOVEDC	0,diwhigh		;#*

	AAddfstrt	AAfetchStart		;same for all displays
        CPMOVEDC	AAvalue,ddfstrt

        CPMOVEDC   	AAbplcon2,bplcon2	;sprite priority over bm
	CPMOVEDC	AAbplcon4,bplcon4	;sprite colors = 240-255

	AAbplcon3	0,1	;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

SWOS2c:	EQU	*+2-SafeWriteCopList1
	CPMOVEDC   BLANKGRAY,color00	;color 0 is $842 dark gray (not black), border??? #*
        CPMOVEDC   BLANKGRAY,color01  
        CPMOVEDC   BLANKGRAY,color02  
        CPMOVEDC   BLANKGRAY,color03  
        CPMOVEDC   BLANKGRAY,color04  
        CPMOVEDC   BLANKGRAY,color05  
        CPMOVEDC   BLANKGRAY,color06  
        CPMOVEDC   BLANKGRAY,color07  
        CPMOVEDC   BLANKGRAY,color08  
        CPMOVEDC   BLANKGRAY,color09  
        CPMOVEDC   BLANKGRAY,color10  
        CPMOVEDC   BLANKGRAY,color11  
        CPMOVEDC   BLANKGRAY,color12  
        CPMOVEDC   BLANKGRAY,color13  
        CPMOVEDC   BLANKGRAY,color14  
        CPMOVEDC   BLANKGRAY,color15

	CPMOVEDC      0,color16	;fill in later #*
	CPMOVEDC      0,color17	;fill in later #*
	CPMOVEDC      0,color18	;fill in later #*
	CPMOVEDC      0,color19	;fill in later #*
	CPMOVEDC      0,color20	;fill in later #*
	CPMOVEDC      0,color21	;fill in later #*
	CPMOVEDC      0,color22	;fill in later #*
	CPMOVEDC      0,color23	;fill in later #*
	CPMOVEDC      0,color24	;fill in later #*
	CPMOVEDC      0,color25	;fill in later #*
	CPMOVEDC      0,color26	;fill in later #*
	CPMOVEDC      0,color27	;fill in later #*
	CPMOVEDC      0,color28	;fill in later #*
	CPMOVEDC      0,color29	;fill in later #*
	CPMOVEDC      0,color30	;fill in later #*
	CPMOVEDC      0,color31	;fill in later #*
  
	AAbplcon3	0,0	;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

        CPMOVEDC   $000,color00   G0=0 R0=0 B2=0 R2=0	;color 0 is $842 dark gray (not black), border??? #*
        CPMOVEDC   $400,color01   G0=0 R0=0 B2=0 R2=1
        CPMOVEDC   $004,color02   G0=0 R0=0 B2=1 R2=0
        CPMOVEDC   $404,color03   G0=0 R0=0 B2=1 R2=1
        CPMOVEDC   $100,color04   G0=0 R0=1 B2=0 R2=0
        CPMOVEDC   $500,color05   G0=0 R0=1 B2=0 R2=1
        CPMOVEDC   $104,color06   G0=0 R0=1 B2=1 R2=0
        CPMOVEDC   $504,color07   G0=0 R0=1 B2=1 R2=1
        CPMOVEDC   $010,color08   G0=1 R0=0 B2=0 R2=0
        CPMOVEDC   $410,color09   G0=1 R0=0 B2=0 R2=1
        CPMOVEDC   $014,color10   G0=1 R0=0 B2=1 R2=0
        CPMOVEDC   $414,color11   G0=1 R0=0 B2=1 R2=1
        CPMOVEDC   $110,color12   G0=1 R0=1 B2=0 R2=0
        CPMOVEDC   $510,color13   G0=1 R0=1 B2=0 R2=1
        CPMOVEDC   $114,color14   G0=1 R0=1 B2=1 R2=0
        CPMOVEDC   $514,color15   G0=1 R0=1 B2=1 R2=1

	CPMOVEDC      0,color16	;fill in later #*
	CPMOVEDC      0,color17	;fill in later #*
	CPMOVEDC      0,color18	;fill in later #*
	CPMOVEDC      0,color19	;fill in later #*
	CPMOVEDC      0,color20	;fill in later #*
	CPMOVEDC      0,color21	;fill in later #*
	CPMOVEDC      0,color22	;fill in later #*
	CPMOVEDC      0,color23	;fill in later #*
	CPMOVEDC      0,color24	;fill in later #*
	CPMOVEDC      0,color25	;fill in later #*
	CPMOVEDC      0,color26	;fill in later #*
	CPMOVEDC      0,color27	;fill in later #*
	CPMOVEDC      0,color28	;fill in later #*
	CPMOVEDC      0,color29	;fill in later #*
	CPMOVEDC      0,color30	;fill in later #*
	CPMOVEDC      0,color31	;fill in later #*

	AAColorPalette	1,1	;#*
	AAColorPalette	1,0	;#*
	AAColorPalette	2,1	;#*
	AAColorPalette	2,0	;#*
	AAColorPalette	3,1	;#*
	AAColorPalette	3,0	;#*
	AAColorPalette	4,1	;#*
	AAColorPalette	4,0	;#*
	AAColorPalette	5,1	;#*
	AAColorPalette	5,0	;#*
	AAColorPalette	6,1	;#*
	AAColorPalette	6,0	;#*

SWOS2d:	EQU	*+2+(16+1)*4-SafeWriteCopList1
	AAColorPalette	7,1	;colors 17-32 = sprite colors #
	AAColorPalette	7,0	;colors 17-32 = sprite colors #

	AAddfstop	AA16fetchStop
	CPMOVEDC	AAvalue,ddfstop

	AAbplcon1	AAscroll,AAscroll
        CPMOVEDC	AAvalue,bplcon1   	;scroll (put it under sprite)

        CPMOVEDC	AA16modulo,bpl1mod		

SWOS3:	EQU	*+2-SafeWriteCopList1
 	CPMOVEDC	$0000,bpl1pth       ;fill in later #*
	CPMOVEDC	$0000,bpl1ptl       ;fill in later #*

SWSPR1	EQU	*+2
          CPMOVEDC   $0000,spr2pth       ;fill in later #*
          CPMOVEDC   $0000,spr2ptl       ;fill in later #*
          CPMOVEDC   $0000,spr3pth       ;fill in later #*
          CPMOVEDC   $0000,spr3ptl       ;fill in later #*
          CPMOVEDC   $0000,spr4pth       ;fill in later #*
          CPMOVEDC   $0000,spr4ptl       ;fill in later #*
          CPMOVEDC   $0000,spr5pth       ;fill in later #*
          CPMOVEDC   $0000,spr5ptl       ;fill in later #*
          CPMOVEDC   $0000,spr6pth       ;fill in later #*
          CPMOVEDC   $0000,spr6ptl       ;fill in later #*
          CPMOVEDC   $0000,spr7pth       ;fill in later #*
          CPMOVEDC   $0000,spr7ptl       ;fill in later #*

SWBPLCON0_1	EQU	*+2-SafeWriteCopList1
	AAbplcon0	1,1,0,0		;depth=1, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes

	CPMOVEDC	AAfetchMode,fmode
	
* do headers HDR3, HDR1, HDR2, HDR0
        CPWAITDC	(21+3),19	;wait to after sprite

*----------------

SWBPLCON0_2	EQU	*+2-SafeWriteCopList1
	AAbplcon0	0,1,0,0		;depth=0, HiRes, non-HAM, no ERSY
	CPMOVEDC	AAvalue,bplcon0	;modes & #planes

	AAbplcon3	0,1	;bank 0, upper nibble
	CPMOVEDC	AAvalue,bplcon3

SWOS3a:	EQU	*+2-SafeWriteCopList1
        CPMOVEDC	BLANKGRAY,color00
	AAbplcon3	0,0	;bank 0, lower nibble
	CPMOVEDC	AAvalue,bplcon3
        CPMOVEDC	$000,color00	;low contrast write

	AAddfstop	AA768fetchStop
        CPMOVEDC	AAvalue,ddfstop

