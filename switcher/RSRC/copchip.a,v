;When Writing, I=Most sig bit of nibble, B, G, R=Least sig bit
FieldWriteCopList2:
	  CPMOVEDC   INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start
          CPMOVEDC   $0000,spr0pth       ;fill in later
          CPMOVEDC   $0000,spr0ptl       ;fill in later
          CPMOVEDC   $0000,spr1pth       ;fill in later
          CPMOVEDC   $0000,spr1ptl       ;fill in later

          CPMOVEDC   $0000,bpl1pth       ;fill in later
          CPMOVEDC   $0000,bpl1ptl       ;fill in later
          CPMOVEDC   $0000,bpl2pth       ;fill in later
          CPMOVEDC   $0000,bpl2ptl       ;fill in later
          CPMOVEDC   $0000,bpl3pth       ;fill in later
          CPMOVEDC   $0000,bpl3ptl       ;fill in later
          CPMOVEDC   $0000,bpl4pth       ;fill in later
          CPMOVEDC   $0000,bpl4ptl       ;fill in later

	  CPMOVEDC   0,cop1lch	;->after Write copper list body, fill in later
          CPMOVEDC   0,cop1lcl	;->after Write copper list body, fill in later

          CPMOVEDC   0,bpl1mod	;-96 for sync write, -4 for 736 wide, 0 for 768 wide, fill in later
          CPMOVEDC   0,bpl2mod

	CPMOVEDC	0,beamcon0	  ;make sure beamcon0 is ok, necessary?
	CPMOVEDC	NEWDENISE,bplcon3 ;fix new Denise right edge problem

;Low contrast write
          CPMOVEDC   $776,color00   I=0 B=0 G=0 R=0
          CPMOVEDC   $876,color01   I=0 B=0 G=0 R=1
          CPMOVEDC   $786,color02   I=0 B=0 G=1 R=0
          CPMOVEDC   $886,color03   I=0 B=0 G=1 R=1
          CPMOVEDC   $778,color04   I=0 B=1 G=0 R=0
          CPMOVEDC   $878,color05   I=0 B=1 G=0 R=1
          CPMOVEDC   $788,color06   I=0 B=1 G=1 R=0
          CPMOVEDC   $888,color07   I=0 B=1 G=1 R=1
          CPMOVEDC   $777,color08   I=1 B=0 G=0 R=0
          CPMOVEDC   $877,color09   I=1 B=0 G=0 R=1
          CPMOVEDC   $787,color10   I=1 B=0 G=1 R=0
          CPMOVEDC   $887,color11   I=1 B=0 G=1 R=1
          CPMOVEDC   $779,color12   I=1 B=1 G=0 R=0
          CPMOVEDC   $879,color13   I=1 B=1 G=0 R=1
          CPMOVEDC   $789,color14   I=1 B=1 G=1 R=0
          CPMOVEDC   $889,color15   I=1 B=1 G=1 R=1

* Low contrast sprite. 
;;          CPMOVEDC   $776,color16   I=0 B=0 G=0 R=0 ;transparent
          CPMOVEDC   $876,color17   I=0 B=0 G=0 R=1
          CPMOVEDC   $786,color18   I=0 B=0 G=1 R=0
          CPMOVEDC   $886,color19   I=0 B=0 G=1 R=1
          CPMOVEDC   $778,color20   I=0 B=1 G=0 R=0
          CPMOVEDC   $878,color21   I=0 B=1 G=0 R=1
          CPMOVEDC   $788,color22   I=0 B=1 G=1 R=0
          CPMOVEDC   $888,color23   I=0 B=1 G=1 R=1
          CPMOVEDC   $777,color24   I=1 B=0 G=0 R=0
          CPMOVEDC   $877,color25   I=1 B=0 G=0 R=1
          CPMOVEDC   $787,color26   I=1 B=0 G=1 R=0
          CPMOVEDC   $887,color27   I=1 B=0 G=1 R=1
          CPMOVEDC   $779,color28   I=1 B=1 G=0 R=0
          CPMOVEDC   $879,color29   I=1 B=1 G=0 R=1
          CPMOVEDC   $789,color30   I=1 B=1 G=1 R=0
          CPMOVEDC   $889,color31   I=1 B=1 G=1 R=1

        CPMOVEDC   ((21<<8)!92),diwstrt
        CPMOVEDC   (((262<<8)&$ff00)!(458-256)),diwstop
          CPMOVEDC   ((64>>1)&$fc),ddfstrt
          CPMOVEDC   ((424>>1)&$fc),ddfstop
          CPMOVEDC   ((7<<4)!7),bplcon1   ;scroll NEEDS TO BE 5.5 ???
          CPMOVEDC   $24,bplcon2         ;sprite priority over bm

FWBPLCON0_2	EQU	*+2
	CPMOVEDC	BPLCON0_DEPTH4,bplcon0	;modes & #planes

FWSPR2	EQU	*+2
          CPMOVEDC   $0000,spr2pth       ;fill in later
          CPMOVEDC   $0000,spr2ptl       ;fill in later
          CPMOVEDC   $0000,spr3pth       ;fill in later
          CPMOVEDC   $0000,spr3ptl       ;fill in later
          CPMOVEDC   $0000,spr4pth       ;fill in later
          CPMOVEDC   $0000,spr4ptl       ;fill in later
          CPMOVEDC   $0000,spr5pth       ;fill in later
          CPMOVEDC   $0000,spr5ptl       ;fill in later
          CPMOVEDC   $0000,spr6pth       ;fill in later
          CPMOVEDC   $0000,spr6ptl       ;fill in later
          CPMOVEDC   $0000,spr7pth       ;fill in later
          CPMOVEDC   $0000,spr7ptl       ;fill in later

          CPENDDC

***********************************************************************
*
* 1plane	HDR3	;line 0 of display	;matte to black
*		HDR1				;pv
*		HDR2				;am/bm
*		HDR0	0planes			
*
* 4planes	WRITE	DT lines >= 0
*		
*		HDR0	0planes
* 1plane	HDR2				;am/bm
*		HDR1				;pv
*		HDR3	0planes			;matte restore 
*
*		NULL 	ZT lines >= 0	
*
* optional end here
*
* 1-4planes	INTERFACE I lines	0planes near end of last line of I
*	
*		NULL	ZB lines >= 0
* 	
* optional end here
*
* 1plane	HDR3			;matte to black
*		HDR1			;pv
*		HDR2			;am/bm
*		HDR0	0planes		
*
* 4planes	WRITE	DB lines >= 0	
*
*		HDR0	0planes		
* 1plane	HDR2			;am/bm
*		HDR1			;pv
*		HDR3			;matte restore 
*		----	;line 240 of display
*
***********************************************************************


DT	set 2
ZT	set 0
I	set 200
ZB	set 0
DB	set 4

SafeWriteCopList1:
	CPMOVEDC	INTF_SETCLR!INTF_COPER,intreq	;copper interrupt = flag start
SWOS1:	EQU	*+2-SafeWriteCopList1
	CPMOVEDC	0,spr0pth	;fill in later #
	CPMOVEDC	0,spr0ptl	;fill in later #
	CPMOVEDC	0,spr1pth	;fill in later #
	CPMOVEDC	0,spr1ptl	;fill in later #

SWOS2:	EQU	*+2-SafeWriteCopList1
	CPMOVEDC	0,cop1lch	;->after Write copper list body, fill in later #
        CPMOVEDC	0,cop1lcl	;->after Wrtte copper list body, fill in later #

	CPMOVEDC	0,beamcon0	  ;make sure beamcon0 is ok, necessary?
	CPMOVEDC   NEWDENISE,bplcon3 	;fix new Denise right edge problem
        CPMOVEDC   ((21<<8)!92),diwstrt

SWOS2b:	EQU	*+2-SafeWriteCopList1
        CPMOVEDC   (((262<<8)&$ff00)!(458-256)),diwstop	;#
        CPMOVEDC   ((64>>1)&$fc),ddfstrt
        CPMOVEDC   $24,bplcon2		;sprite priority over bm

SWOS2c:	EQU	*+2-SafeWriteCopList1
	CPMOVEDC	$0,color00	;# $842 dark gray (not black), border

* Low contrast sprite. 
;;        CPMOVEDC   $776,color16   I=0 B=0 G=0 R=0 ;transparent
        CPMOVEDC   $876,color17   I=0 B=0 G=0 R=1
        CPMOVEDC   $786,color18   I=0 B=0 G=1 R=0
        CPMOVEDC   $886,color19   I=0 B=0 G=1 R=1
        CPMOVEDC   $778,color20   I=0 B=1 G=0 R=0
        CPMOVEDC   $878,color21   I=0 B=1 G=0 R=1
        CPMOVEDC   $788,color22   I=0 B=1 G=1 R=0
        CPMOVEDC   $888,color23   I=0 B=1 G=1 R=1
        CPMOVEDC   $777,color24   I=1 B=0 G=0 R=0
        CPMOVEDC   $877,color25   I=1 B=0 G=0 R=1
        CPMOVEDC   $787,color26   I=1 B=0 G=1 R=0
        CPMOVEDC   $887,color27   I=1 B=0 G=1 R=1
        CPMOVEDC   $779,color28   I=1 B=1 G=0 R=0
        CPMOVEDC   $879,color29   I=1 B=1 G=0 R=1
        CPMOVEDC   $789,color30   I=1 B=1 G=1 R=0
        CPMOVEDC   $889,color31   I=1 B=1 G=1 R=1

        CPMOVEDC   ((72>>1)&$fc),ddfstop
        CPMOVEDC   ((3<<4)!3),bplcon1   ;scroll
        CPMOVEDC   -8,bpl1mod		

SWOS3:	EQU	*+2-SafeWriteCopList1
 	CPMOVEDC	$0000,bpl1pth       ;fill in later #
	CPMOVEDC	$0000,bpl1ptl       ;fill in later #

SWSPR1	EQU	*+2
          CPMOVEDC   $0000,spr2pth       ;fill in later #
          CPMOVEDC   $0000,spr2ptl       ;fill in later #
          CPMOVEDC   $0000,spr3pth       ;fill in later #
          CPMOVEDC   $0000,spr3ptl       ;fill in later #
          CPMOVEDC   $0000,spr4pth       ;fill in later #
          CPMOVEDC   $0000,spr4ptl       ;fill in later #
          CPMOVEDC   $0000,spr5pth       ;fill in later #
          CPMOVEDC   $0000,spr5ptl       ;fill in later #
          CPMOVEDC   $0000,spr6pth       ;fill in later #
          CPMOVEDC   $0000,spr6ptl       ;fill in later #
          CPMOVEDC   $0000,spr7pth       ;fill in later #
          CPMOVEDC   $0000,spr7ptl       ;fill in later #

