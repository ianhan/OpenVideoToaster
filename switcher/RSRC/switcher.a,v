2.198
log
@re-wrote input handler to eat amiga-m/n's is moving to
or away from the switcher screen, at which time it will
signal the switcher task to do a screen change.  This
eliminates race conditions between the switcher task
and the input handler.
@
text
@d7 1
a7 1
* $Id: switcher.a,v 2.197 1994/12/09 22:16:45 Kell Exp pfrench $
d10 7
d2763 21
a2783 21
* Create a dummy NULL Software Interrupt handler.
* This will remain in memory, even after the Switcher is Exited ****!!!!!

	move.l	(_AbsExecBase).w,a6
	move.l	#NullSoftIntHandlerEnd-NullSoftIntHandler,d0
	moveq	#0,d1
	CALLROM	AllocMem

	PUT.l	d0,TB_NullSoftIntHandler
	bne.s	600$
	PUT.l	#NullSoftIntHandler,TB_NullSoftIntHandler	;default if can't allocmem, though this may crash when switcher is exited ***!!!!
	bra.s	666$

600$	movea.l	d0,a0
	lea	NullSoftIntHandler(pc),a1
	move.l	#((NullSoftIntHandlerEnd-NullSoftIntHandler)/2)-1,d0
	
610$	move.w	(a1)+,(a0)+
	dbra	d0,610$
	
	CALLROM	CacheClearU		;make sure it's flushed from cache
d2790 4
a2793 4
NullSoftIntHandler:
	move.w	#INTF_SOFTINT,_custom+intreq ;clear it out
	RTS
NullSoftIntHandlerEnd:
d6782 1
a6782 1
	dc.w	0			;TB_pad2
d6788 1
a6788 1
	dc.l	0			;TB_NullSoftIntHandler ->dummy handler that gets left in memory after switcher exit!
@


2.197
log
@More singlesteps
@
text
@d7 1
a7 1
* $Id: switcher.a,v 2.196 1994/12/09 15:47:46 pfrench Exp Kell $
d10 3
a3967 6
*					; Addendum 1/4/91:
*					; disable hotkey if a Switcher
*					; requester is active
	btst.b	#REQACTI_ONOFF_BIT,TB_Flags(a1)
	bne.s	.ExitQuickly

d3979 3
a3981 7
	move.l	HotKeyCount(a1),a2		; get current HotKey code
	move.b	(a2)+,d0			; in HotKey sequence
	move.b	ie_Code+1(a0),d1		; get current key code
	cmp.b	d0,d1				; do they match?
	beq.s	.AHotKey			; if they do - skip on to
*						; check on if the HotKey
*						; sequence was completed
d3983 2
a3984 5
	move.b	d1,d1			; key up?
	bmi	.IgnoreIt		; ignore it if so

	lea	HotKeySeq(a1),a2	; else the HotKey sequence was broken
	move.l	a2,HotKeyCount(a1)	; - reset the HotKey sequence
d3986 1
a3986 4
	tst.w	TB_DisplayState(a1)	; are we in Switcher display state?
;;$$	bne	.IgnoreIt		; if not - skip out
;;$$
	beq.s	.DoSwitcherSafeGuard	; if so - jump to it
d3988 4
a3991 4
	move.l	TB_ITUBase(a1),a2	; else at WB - insure that no pressed
	move.l	ib_ActiveWindow(a2),a2	; keys manage to make it to the
	cmp.l	TB_Window(a1),a2	; Switcher window
	bne	.IgnoreIt
a3992 1
	clr.b	ie_Class(a0)
d3995 3
a3997 2
.DoSwitcherSafeGuard:
;;$$
d3999 2
a4000 6
;;	cmp.b	#$36,d1			; else don't allow a Left AMIGA - N
;;	bne	.IgnoreIt			; to be seen by intuition
*						; Addendum 4/25/91: do not
*						; allow LEFT AMIGA - any key
*						; be seen by intuition
*						; (2 commented lines above)
d4002 2
a4003 24
	move.w	ie_Qualifier(a0),d0		; as this would cause
;	and.w	#IEQUALIFIER_LCOMMAND,d0	; intuition to bring up the
	btst	#IEQUALIFIERB_LCOMMAND,d0
	beq	.IgnoreIt			; WB screen to the display
	btst	#IEQUALIFIERB_LALT,d0
	bne	.IgnoreIt			;pass through key equiv

	clr.b	ie_Class(a0)			; front
	bra	.IgnoreIt			; (and you won't see it)

.AHotKey:
	move.l	a2,d1
	lea	HotKeyLast1(a1),a2	; was the last key in the HotKey
	cmp.l	a2,d1			; sequence reached?
	beq.s	.FireUp			; yup - skip to do HotKey stuff

	move.l	d1,HotKeyCount(a1)	; else save pointer to next HotKey
*					; in the HotKey sequence
	bra	.IgnoreIt		; and skip on to continue

************* HOT KEY RETURN TO SWITCHER ************************
.FireUp:
	lea	HotKeySeq(a1),a2	; reset the HotKey sequence for the
	move.l	a2,HotKeyCount(a1)	; next HotKey sequence
d4005 1
a4005 5
	SAVE	a0/a1/a5/a6		; save extra work registers
	move.l	a1,a5
	tst.w	TB_DisplayState(a5)	; determine to what display state
*					; we're to be going to
	beq	.GotoWB			; if to WB screen - jump on
d4007 2
a4008 2
*					; else to Toaster display
	clr.w	TB_DisplayState(a5)	; denote we're at Toaster display
d4010 3
a4012 6
* Doing this here prevents the problem of the operating system doing
* weird things when it thinks a NONLACEed screen is being displayed
* when we may actually have a LACED Toaster copperlist being shown.
	GET.l	TB_ITUBase,a6
	GET.l	TB_Screen,a0
	CALLROM	ScreenToFront	;near bottom of display = LACEed
d4014 4
a4017 1
	XJSR	ReinstallCurrentCopList
d4019 3
a4021 2
	moveq	#0,d0
	CALLTL	LockToasterELH
d4023 1
a4023 6
;;	DEA.l	TB_VTSetUp,a0
;;	ELHSET_WERB	a0
;;	ELHSET_NOLOCK 	a0
;;* This assumes that the Sprite will be sent out on Amiga line 21 (?) which
;;* is the top of overscan.  This maynot be true if HotKeyed from CG.
;;	CALLTL	SendELH2Toaster
d4025 2
a4026 2
	CALL	AutoRequestDisable	; Addendum 3/21/90: disarm the
*					; AutoRequest function
d4028 2
a4029 1
	CALL	DisableKeyRepeat	; Addendum 6/7/90:
d4031 2
a4032 9
;;	GET.l	TB_ITUBase,a6
	GET.l	TB_Screen,a0
	CLEAR	d0
	move.l	#-4000,d1
	CALLROM	MoveScreen
	GET.l	TB_Screen,a0		; assure Toaster screen at front
	sub.w	#SCREENFUDGEX,sc_LeftEdge(a0)	; reposition mouse in
	sub.w	#SCREENFUDGEY,sc_TopEdge(a0)	; relation to the screen
;;	CALLROM	ScreenToFront	
d4034 3
a4036 1
;; Bug 3.0 Crash	CALLROM	CloseWorkBench		; attempt to insure WB closed
d4038 2
a4039 3
*					; re-adjust the SoftSprite
	XCALL	SoftSpriteAudioOn	; interrupt trigger and turn audio
*					; DMA back on along with SoftSprite
d4041 3
a4043 8
	CLEAR	d0			; now insure that the mouse is
	move.l	(sp),a0			; positioned properly
	move.b	#IECLASS_RAWMOUSE,ie_Class(a0)
	move.b	d0,ie_SubClass(a0)
	move.w	#IECODE_NOBUTTON,ie_Code(a0)
	move.w	#IEQUALIFIER_RELATIVEMOUSE,ie_Qualifier(a0)
	move.w	d0,ie_X(a0)
	move.w	d0,ie_Y(a0)
d4045 1
a4045 1
	bra.s	.Continue		; jump to continue
a4046 2
.GotoWB:
 IFND	SPECIAL_DEBUG
a4049 15
	GET.l	TB_ITUBase,a0		; insure that the Switcher screen
	move.l	ib_FirstScreen(a0),a0	; is the active one - else nogo
	cmp.l	TB_Screen(a5),a0
	bne.s	.Continue

;;$$
	GET.l	TB_Window,a0		; insure only Switcher or Config
	move.l	wd_FirstGadget(a0),a0	; Slice active or nogo
	move.w	gg_GadgetID(a0),d0
	cmp.w	#'SW',d0
;;;;	beq.s	.OKToDoIt
;;;;	cmp.w	#'CF',d0
	bne.s	.Continue

;;;;.OKToDoIt:
d4056 1
a4056 1
	GET.l	TB_SwitcherTask,a1
d4059 1
a4059 2
;;$$
 ENDC	; SPECIAL_DEBUG
d4061 3
a4063 25
 IFD	SPECIAL_DEBUG
	GET.l	TB_ITUBase,a6
	CALLROM	OpenWorkBench		; check to make sure WB to goto
	move.l	d0,d0
	beq.s	.Continue		; if WB didn't open

	PUT.w	#-1,TB_DisplayState	; denote WB display state
	CALL	AutoRequestEnable	; Addendum 3/21/90: re-arm the
*					; AutoRequest function

	CALL	EnableKeyRepeat		; Addendum 6/7/90:

	GET.l	TB_Screen,a0
	add.w	#SCREENFUDGEX,sc_LeftEdge(a0)	; reposition mouse in
	add.w	#SCREENFUDGEY,sc_TopEdge(a0)	; relation to the screen
	CLEAR	d0
	move.l	#4000,d1
	CALLROM	MoveScreen
	XCALL	SoftSpriteAudioOff	; turn the SoftSprite completely off
;;$$
	XCALL	CancelCG		; assure that the CG has no
*					; transparency planes up - if active
	XCALL	CancelNonStdEfx		; Get out of any non-standard effects
	moveq	#1,d0
	CALLTL	LockToasterELH
d4065 1
a4065 3
	CALLROM	WBenchToFront		; insure WB brought up to the front
	XCALL	RestoreCopperList	; restore AMIGA's display
 ENDC	; SPECIAL_DEBUG
d4068 1
a4068 1
	REST	a0/a1/a5/a6		; restore work registers
@


