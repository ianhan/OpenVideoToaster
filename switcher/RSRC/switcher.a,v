SwitAlreadyRunningExit:

	GET.l	TB_SwitcherTask,a0		; get our Process
d1346 1
a1346 1

d1398 12
d1472 1
a1472 1
	bcs	.NoAutoHue
a1478 48

***********************************
	movem.l	a0-a1/d0-d2,-(sp)

	GET.l	TB_SYSBase,a6		; see if Switcher program is already
	CALLROM	Forbid			; running - no more than 1 copy can
*					; be allowed to run

	DEA	DevHPortName,a1
	CALLROM	FindPort
	move.l	d0,d2

	STEPMSG	<before Permit>

	CALLROM	Permit

	move.l	d2,d2			; a copy already running?

	movem.l	(sp)+,a0-a1/d0-d2

	beq.s	.xxFirstCopy		; if not - we should be the first

	; HACK HACK HACK to exit early if already running

	SETERR	7
	REST	d2/d3/a2/a3/a6		; Restore these registers
	lea	SwitAlreadyRunningExit(pc),a0
	move.l	a0,(sp)			; return address (OverWrite InitializeAll rts)
	rts
***********************************
.xxFirstCopy
	movem.l	a0-a1/d0-d1,-(sp)
*	At one time this happened right before project loading.

*					; ToasterBase should be initialized
*					; - make it public by installing its
	GET.l	TB_SYSBase,a6		; node in the Exec library list
	CALLROM	Forbid
	lea	LibList(a6),a0
	move.l	a5,a1
	CALLROM	AddTail
	CALLROM	Permit
	PUT.w	#-1,TB_InList		; denote ToasterBase installed in
					; Exec's library list

	movem.l	(sp)+,a0-a1/d0-d1
***********************************

