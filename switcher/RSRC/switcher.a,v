elhlistconf
;;	dc.b	EH5,EH6A,EH6B,EHEND
	dc.b	EH1,EH2,EH3,EH4,EH5,EH68,EH69,EH6A,EH6B,EHEND

FrontEndName:	DC.b	'FrontEnd',0

*	Addendum 10/17/90:

ToasterLogoName:

	IFD	OLDTOASTER
	DC.b	'Switcher_Support/StartupFrames/VTLogo',0
	ENDC

	IFND	OLDTOASTER
