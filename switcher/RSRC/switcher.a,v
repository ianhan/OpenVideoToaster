	DC.w	11			; ta_YSize (was 12)
	DC.b	FS_NORMAL!FSF_COLORFONT	; ta_Style (was 0)
	DC.b	FPF_DISKFONT		; ta_Flags (was 0)

	DATASYM	GFXBaseName,DataPool
