CRLIB_OBJ = \
	crlib:obj/crouton.o \
	crlib:obj/smartstring.o \
	crlib:obj/croutondos.o \
	crlib:obj/misc.o \
	crlib:obj/filetypes.o \
	crlib:obj/bufferdos.o \
	crlib:obj/CroutonDefs.o \
	crlib:obj/FilePaths.o \
	crlib:obj/BitMaps.o \
	crlib:obj/SerialDebug.o

lib: crlib:exe/crouton.library crlib:pragma/crouton.h \
	crlib:libinc/crouton_lib.i tinc:crouton_lib.i libs:crouton.library

libs:crouton.library: crlib:exe/crouton.library
	copy crlib:exe/crouton.library libs: quiet clone
	avail >nil: FLUSH
	echo "crouton.library copied to libs:"

crlib:exe/crouton.library: crlib:bat/LastUpdateDate $(CRLIB_OBJ) crlib:fd/crouton.fd
	slink libfd crlib:fd/crouton.fd from lib:libent.o lib:libinit.o $(CRLIB_OBJ) \
	MAP crlib:crouton.map MLIB MSYM MXREF \
	to ram:crouton.library libVersion 4 libRevision 0 lib lib:sc.lib
	@@copy ram:crouton.library $@@ quiet clone
	@@delete ram:crouton.library quiet

crlib:pragma/crouton.h: crlib:fd/crouton.fd
	fd2pragma crlib:fd/crouton.fd crlib:pragma/crouton.h
# 	@@NextTime crlib:fd/crouton.fd crlib:pragma/crouton.h

crlib:libinc/crouton_lib.i: crlib:fd/crouton.fd
	execute crlib:bat/fd2asm
# 	@@NextTime crlib:fd/crouton.fd crlib:libinc/crouton_lib.i

tinc:crouton_lib.i: crlib:libinc/crouton_lib.i
# it will be put into switcher/sinc
	copy crlib:libinc/crouton_lib.i tinc: quiet clone
# 	@@NextTime crlib:libinc/crouton_lib.i tinc:crouton_lib.i

# This line is to support testmake
ram:NeverExists: lib
	quit 20

#*******************************************************************
# Remember to use LastUpdateDate dependancy on all slinks!!
# only does this if LastUpdateDate doesn't exist
crlib:bat/LastUpdateDate:
	date >crlib:bat/LastUpdateDate

#*******************************************************************
# Compiling rules.

LIBSAS = RESETOPTIONS NOSTKCHK STRMERGE UNSCHAR ERRORREXX LIBCODE\
	NOVER IDIR=scinc: IDIR=Include.h: IDIR=tinc:	\
	IDIR=crlib:inc/ IDIR=crlib:proto/ IDIR=crlib:libinc/
#	DATA=FAR CODE=FAR \
#	GLOBALSYMBOLTABLE=tsrc:GST/amiga.gst

.c.o:
	sc $(LIBSAS) OBJNAME=ram:$>.o $*.c
	@@copy ram:$>.o $@@ quiet clone
	@@delete ram:$>.o quiet
# 	@@NextTime $*.c ram:temp
#*******************************************************************

crlib:printbitmap: crlib:printbitmap.c
	sc LINK STRIPDEBUG crlib:printbitmap.c

crlib:inc/bitmaps.h: $(CRLIB_IMAGES) crlib:printbitmap
	crlib:bat/makebitmaps

crlib:obj/bitmaps.o: crlib:src/bitmaps.c crlib:inc/bitmaps.h
	sc RESETOPTIONS NOVER NOSTKCHK STRMERGE UNSCHAR ERRORREXX LIBCODE OBJNAME=ram:$>.o \
		IDIR=Include.h: $*.c
	@@copy ram:$>.o $@@ quiet clone
	@@delete ram:$>.o quiet

crlib:obj/crouton.o: crlib:src/crouton.c crlib:libinc/listhelp.h crlib:bat/makefile

crlib:obj/filetypes.o: crlib:src/filetypes.c crlib:inc/bitmaps.h crlib:libinc/croutons.h crlib:bat/makefile

crlib:obj/smartstring.o: crlib:src/smartstring.c crlib:libinc/listhelp.h crlib:bat/makefile

crlib:obj/croutondos.o: crlib:src/croutondos.c crlib:libinc/listhelp.h crlib:bat/makefile

crlib:obj/bufferdos.o: crlib:src/bufferdos.c crlib:libinc/croutondos.h crlib:bat/makefile

crlib:obj/filepaths.o: crlib:src/filepaths.c crlib:bat/makefile

#********************************************
# for HiSoft assembler, options in assembler.i
HISOFT = -Icrlib:inc/ -D -L -O
HIERRS = >crlib:hisoft.errs
# Not all versions like include stuff above...
GENIM = GenIm2.lastwork

.a.o:
	$(GENIM) $(HIERRS) $*.a $(HISOFT)ram:temp
#	@@NextTime $*.a ram:temp
	@@copy ram:temp $@@ quiet clone
	@@delete ram:temp quiet
#********************************************


crlib:obj/misc.o: crlib:src/misc.a crlib:bat/makefile

crlib:obj/CroutonDefs.o: crlib:src/CroutonDefs.a crlib:bat/makefile

crlib:obj/SerialDebug.o: crlib:src/SerialDebug.a

